	/******************************************
	 * 정평지표 정보 가져오기
	 ******************************************/
	function getMetricInfo() {
		var year 		= J("#year").val();
		var govMetricId = J("#govMetricId").val();
		
		J.ajax({
			url        : "$!{context_path}/gov/bos/bosMetric/govMetricInfo_ajax.vw",
        	type       : "POST",
			data       : "year=" + year + "&amp;govMetricId=" + govMetricId,
        	dataType   : "xml",
			contentType: "application/x-www-form-urlencoded; charset=UTF-8",
        	success    : function (xml) {
				var bossMetricNm 	= "";
				var direcId 		= "";
				var evalMethodId 	= "";
				var gbnId			= "";
        		var unit			= "";
				var actCalType		= "";
        		var insertUserId	= "";
        		var insertUserNm	= "";
        		var scDeptId		= "";
        		var scDeptNm		= "";
        		var weight			= "";
        		var sortOrder		= "";
        		var useYn			= "";
        		var content			= "";
				var baseValue		= "";
				var bestTarget		= "";
				var lowestTarget	= "";
				var stadConst		= "";
				
				var lowestTarget3year	= "";
				var bestTarget3year	= "";
				var actual3year		= "";
				var rateYear		= "";
				
				var detailEvalSummaryComment = "";
				
			
				if(J(xml).find("items").find("item").length > 0) {
					J(xml).find("items").find("item").each(function() {
						bossMetricNm 	= J(this).find("govMetricNm").text();
						direcId 		= J(this).find("direcId").text();
						evalMethodId 	= J(this).find("evalMethodId").text();
						gbnId 			= J(this).find("gbnId").text();
						unit 			= J(this).find("unit").text();
						actCalType 		= J(this).find("actCalType").text();
						insertUserId 	= J(this).find("insertUserId").text();
						insertUserNm 	= J(this).find("insertUserNm").text();
						scDeptId 		= J(this).find("scDeptId").text();
						scDeptNm 		= J(this).find("scDeptNm").text();
						weight 			= J(this).find("weight").text();
						sortOrder 		= J(this).find("sortOrder").text();
						useYn 			= J(this).find("useYn").text();
						content 		= J(this).find("content").text();
						baseValue 		= J(this).find("baseValue").text();
						bestTarget 		= J(this).find("bestTarget").text();
						lowestTarget 	= J(this).find("lowestTarget").text();
						stadConst 		= J(this).find("stadConst").text();
						
						lowestTarget3year	= J(this).find("lowestTarget3year").text();
						bestTarget3year	= J(this).find("bestTarget3year").text();
						actual3year 	= J(this).find("actual3year").text();
						rateYear 		= J(this).find("rateYear").text();
						
						detailEvalSummaryComment = J(this).find("detailEvalSummaryComment").text();
						
						J("#bossMetricNm").val(bossMetricNm);
						J("#direcId").val(direcId);
						J("#evalMethodId").val(evalMethodId);
						J("#gbnId").val(gbnId);
						J("#unit").val(unit);
						J("#actCalType").val(actCalType);
						J("#insertUserId").val(insertUserId);
						J("#insertUserNm").val(insertUserNm);
						J("#scDeptId").val(scDeptId);
						J("#scDeptNm").val(scDeptNm);
						J("#weight").val(weight);
						J("#sortOrder").val(sortOrder);
						J("#useYn").val(useYn);
						J("#content").val(content);
						J("input[name=baseValue]").val(baseValue);
						J("input[name=bestTarget]").val(bestTarget);
						J("input[name=lowestTarget]").val(lowestTarget);
						J("input[name=stadConst]").val(stadConst);
						
						J("input[name=lowestTarget3year]").val(lowestTarget3year);						
						J("input[name=bestTarget3year]").val(bestTarget3year);						
						J("input[name=actual3year]").val(actual3year);						
						J("input[name=rateYear]").val(rateYear);						
						
						J("#detailEvalSummaryComment").val(detailEvalSummaryComment);
						
					});
					
					chgMethod();
					if(gbnId=="01"){
						checkModifyCalType();
						var cnt = 0;
                		J(xml).find("items").find("coldata").each(function() {
							J('input[name=calTypeCols]').eq(cnt).val(J(this).find("colId").text());
        					J('input[name=calTypeColNms]').eq(cnt).val(J(this).find("colNm").text());
							J('select[name=units]').eq(cnt).val(J(this).find("colUnit").text());
							
							//J('input[name=calTypeColNms]').eq(cnt).attr("disabled", true);
							//J('input[name=units]').eq(cnt).attr("disabled", true);
							cnt++;
						})
					}else if(gbnId=="02"){
						var map = J("input[name=check]", "#actDetailArea");
        				J.each(map, function(key, value){
            				J(value).attr("checked","checked");
            			});	
						delDetail();
			
						var cnt = 0;
                		J(xml).find("items").find("detaildata").each(function() {
							addDetail();
							J('textarea[name=detailEvalContents]').eq(cnt).text(J(this).find("detailEvalContent").text());
        					J('textarea[name=evalContents]').eq(cnt).text(J(this).find("evalContent").text());
							J('input[name=detailUserIds]').eq(cnt).val(J(this).find("detailUserId").text());
							J('input[name=detailUserNms]').eq(cnt).val(J(this).find("detailUserNm").text());
							cnt++;
						})
						
					}
				}
			},  
			error : function(xhr, status, error) {
				alert(error);
			}
    	});
		
        setMetricDisabled(true);        		
	}
	
	/******************************************
	 * 정평지표 정보 DISABLED
	 ******************************************/
	function setMetricDisabled(gubun) {
		var tGubun = gubun;
		setTimeout(function(){
    		J("#direcId").attr("disabled", tGubun);
    		J("#evalMethodId").attr("disabled", tGubun);
    		J("#gbnId").attr("disabled", tGubun);
    		J("#unit").attr("disabled", tGubun);
			
			if(J("#gbnId").val()=="01"){
				J("#actCalType").attr("disabled", tGubun);
				
				/*
				J("input[name=baseValue]").attr("disabled", tGubun);
				J("input[name=bestTarget]").attr("disabled", tGubun);
				J("input[name=lowestTarget]").attr("disabled", tGubun);
				J("input[name=stadConst]").attr("disabled", tGubun);
				
				J("input[name=lowestTarget3year]").attr("disabled", tGubun);
				J("input[name=bestTarget3year]").attr("disabled", tGubun);
				J("input[name=actual3year]").attr("disabled", tGubun);
				J("input[name=rateYear]").attr("disabled", tGubun);
				*/
						
				J('input[name=calTypeColNms]').attr("disabled", tGubun);
				J('select[name=units]').attr("disabled", tGubun);
				
			}else{
				J('textarea[name=detailEvalContents]').attr("disabled", tGubun);
				J('textarea[name=evalContents]').attr("disabled", tGubun);
							
				J("#detailEvalSummaryComment").attr("disabled", tGubun);
						
			}
		},500);
	}
	
	/******************************************
	 * 정평지표 정보 DISABLED
	 ******************************************/
	function setMetricDisabledNo(gubun) {
		var tGubun = gubun;
		J("#direcId").attr("disabled", tGubun);
		J("#evalMethodId").attr("disabled", tGubun);
		J("#gbnId").attr("disabled", tGubun);
		J("#unit").attr("disabled", tGubun);
		
		if(J("#gbnId").val()=="01"){
			J("#actCalType").attr("disabled", tGubun);
			J("input[name=baseValue]").attr("disabled", tGubun);
			J("input[name=bestTarget]").attr("disabled", tGubun);
			J("input[name=lowestTarget]").attr("disabled", tGubun);
			J("input[name=stadConst]").attr("disabled", tGubun);
			
			J("input[name=rateYear]").attr("disabled", tGubun);
			J("input[name=actual3year]").attr("disabled", tGubun);
			J("input[name=bestTarget3year]").attr("disabled", tGubun);
			J("input[name=lowestTarget3year]").attr("disabled", tGubun);
					
			J('input[name=calTypeColNms]').attr("disabled", tGubun);
			J('select[name=units]').attr("disabled", tGubun);
			
		}else{
			J('textarea[name=detailEvalContents]').attr("disabled", tGubun);
			J('textarea[name=evalContents]').attr("disabled", tGubun);
						
			J("#detailEvalSummaryComment").attr("disabled", tGubun);
					
		}
	}

//]]>
</script>    
 
