							{name:'STRATEGY_NM'			,index:'STRATEGY_NM'        ,width:80   ,align:'left'   },
                            {name:'METRIC_ID'          	,index:'METRIC_ID'          ,width:80   ,align:'left'   },
							{name:'UP_METRIC_ID'        ,index:'UP_METRIC_ID'       ,width:80   ,align:'left'   },
							{name:'METRIC_NM'          	,index:'METRIC_NM'          ,width:200   ,align:'left'   ,formatter:linkUpdateMetric },
                            {name:'EVAL_CYCLE_NM'       ,index:'EVAL_CYCLE_NM'      ,width:40    ,align:'center' },
							{name:'WEIGHT'              ,index:'WEIGHT'             ,width:50    ,align:'center' },
							{name:'UNIT_NM'             ,index:'UNIT_NM'            ,width:40    ,align:'center' },
                            {name:'TARGET'              ,index:'TARGET'             ,width:70   ,align:'right' }, 
                            {name:'ACTUAL'            	,index:'ACTUAL'            	,width:70   ,align:'right' ,formatter:linkUpdateActual },
							{name:'SCORE'            	,index:'SCORE'            	,width:50    ,align:'right'  },
                            {name:'STATUS'        		,index:'STATUS'            	,width:40    ,align:'center' ,formatter:linkUpdateSignal }
							],
			rowNum       : $!{jqGrid_rownum},
			autowidth    : false, 
			viewrecords  : true,
			loadonce     : false,
			multiselect  : false,
