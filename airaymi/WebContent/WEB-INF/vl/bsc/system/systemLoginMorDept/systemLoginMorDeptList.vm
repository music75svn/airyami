#set($curr_url = $!{context_path} + "/bsc/system/systemLoginMorDept/systemLoginMorDeptList.vw")
<script type="text/javascript" language="javascript" >
//<![CDATA[

	var J = jQuery;

	J(document).ready(function(){

		/******************************************
		 * 데이터 그리드 출력
		 ******************************************/
		var str = J('#formFind').serialize();

		jQuery("#list").jqGrid({
			url          :"$!{context_path}/bsc/system/systemLoginMorDept/systemLoginMorDeptList_xml.vw" + "?" + str,
			mtype        :"POST",
			datatype     :"json",
			jsonReader   : {
    						page   : "page",
    						total  : "total",
    						root   : "rows",
    						records: function(obj){return obj.length;},
    						repeatitems: false,
    						id     : "id"
    				       },
			height       : 220,
			width        : $!{jqGrid_width},
			colNames     :['$!{SC_DEPT_NM}코드','$!{SC_DEPT_NM}', '1월', '2월', '3월', '4월', '5월', '6월', '7월', '8월', '9월', '10월', '11월', '12월'],
			colModel     :[
                            {name:'SC_DEPT_ID'           ,index:'SC_DEPT_ID'           ,width:150   ,align:'center' },
                            {name:'SC_DEPT_NM'           ,index:'SC_DEPT_NM'           ,width:210   ,align:'left' },
                            {name:'A'                    ,index:'A'                    ,width:60   ,align:'center' ,formatter:linkUpdate1 },
                            {name:'B'                    ,index:'B'                    ,width:60   ,align:'center' ,formatter:linkUpdate2 },
                            {name:'C'                    ,index:'C'                    ,width:60   ,align:'center' ,formatter:linkUpdate3 },
                            {name:'D'                    ,index:'D'                    ,width:60   ,align:'center' ,formatter:linkUpdate4 },
                            {name:'E'                    ,index:'E'                    ,width:60   ,align:'center' ,formatter:linkUpdate5 },
                            {name:'F'                    ,index:'F'                    ,width:60   ,align:'center' ,formatter:linkUpdate6 },
                            {name:'G'                    ,index:'G'                    ,width:60   ,align:'center' ,formatter:linkUpdate7 },
                            {name:'H'                    ,index:'H'                    ,width:60   ,align:'center' ,formatter:linkUpdate8 },
                            {name:'I'                    ,index:'I'                    ,width:60   ,align:'center' ,formatter:linkUpdate9 },
                            {name:'J'                    ,index:'J'                    ,width:60   ,align:'center' ,formatter:linkUpdate10},
                            {name:'K'                    ,index:'K'                    ,width:60   ,align:'center' ,formatter:linkUpdate11},
                            {name:'L'                    ,index:'L'                    ,width:60   ,align:'center' ,formatter:linkUpdate12}
							],
			rowNum       : $!{jqGrid_rownum},
			autowidth    : false,
			viewrecords  : true,
			loadonce     : true,
			multiselect  : false,
			cellEdit     : true,
			caption:"시스템 사용현황(조직별)"
		}).jqGrid('hideCol',"SC_DEPT_ID").setGridWidth($!{jqGrid_width});

		/******************************************
		 * 조회
		 ******************************************/
		 J("#formFind").submit(function(event) {
			//submit 하기전 작업추가

		});

		/******************************************
		 * 비교그룹 변경
		 ******************************************/
		J("#compareGrp").change(function() {
			J("#findScDeptId").val("");
			J("#findScDeptNm").val("");
		});

    	/******************************************
    	 * 조직선택 팝업호출
    	 ******************************************/
    	J("#icSearch").fancybox();
    	J("#anchor").fancybox();      //상세팝업

	});

	/******************************************
	 * jqGrid 수정페이지 링크
	 ******************************************/
	function linkUpdate1(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','01')>" + cellvalue + "</a>"; }
	function linkUpdate2(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','02')>" + cellvalue + "</a>"; }
	function linkUpdate3(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','03')>" + cellvalue + "</a>"; }
	function linkUpdate4(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','04')>" + cellvalue + "</a>"; }
	function linkUpdate5(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','05')>" + cellvalue + "</a>"; }
	function linkUpdate6(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','06')>" + cellvalue + "</a>"; }
	function linkUpdate7(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','07')>" + cellvalue + "</a>"; }
	function linkUpdate8(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','08')>" + cellvalue + "</a>"; }
	function linkUpdate9(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','09')>" + cellvalue + "</a>"; }
	function linkUpdate10(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','10')>" + cellvalue + "</a>"; }
	function linkUpdate11(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','11')>" + cellvalue + "</a>"; }
	function linkUpdate12(cellvalue, options, rowObject) { return "<a href=javascript:goModify('" + options.rowId +"','12')>" + cellvalue + "</a>"; }

	/******************************************
	 * 수정
	 ******************************************/
	function goModify(rowId, rowObject) {
		var ret = J("#list").jqGrid('getRowData',rowId);
		J("#anchor").attr("href","$!{context_path}/bsc/system/systemLoginMorDept/systemLoginMorDeptModify.vw?findYear=${searchMap.txt2html("findYear")}&amp;findMon="+rowObject+"&amp;findScDeptId="+ret.SC_DEPT_ID+"&amp;includeAdmin=${searchMap.txt2html("includeAdmin")}");
		J("#anchor").click();
	}

	/*****************************************************
	 * 조직선택 설정
	 *****************************************************/
	function setDeptInfoPop(scDeptId, scDeptNm) {

		scDeptNm = J(scDeptNm).text();
		J("#findScDeptId").val(scDeptId);
		J("#findScDeptNm").val(scDeptNm);
		J("#compareGrp").val("");
		J.fancybox.close();
	}

	/*****************************************************
	 * 조직선택 취소
	 *****************************************************/
	function cancleDept() {
		J("#findScDeptId").val("");
		J("#findScDeptNm").val("");
	}

//]]>
</script>

