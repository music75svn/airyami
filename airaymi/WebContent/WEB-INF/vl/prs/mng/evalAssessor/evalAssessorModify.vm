	 ******************************************/
	function goClickAction(deptId, deptNm) {
    	var findEmpn = J("#findEmpn").val();
    	var findKorNm = J("#findKorNm").val();
    	J("#findSelectDept").val(deptId);
    	
		J.ajax({
        	url      : "$!{context_path}/prs/mng/evalAssessor/evalAssessor_ajax.vw",
        	type     : "GET", 
			data     : "deptId=" + deptId +"&amp;findYear=" +J("#findYear").val(), 
        	dataType : "xml",
			contentType : "application/xml",
        	success  : function (xml) {
				J("#source1").empty();
				if(J(xml).find("items").find("item").length > 0) {
					$(xml).find("items").find("item").each(function() {  
                    	var userId = $(this).find("empn").text();  
                    	var userNm = $(this).find("korNm").text();
                    	var castNm = $(this).find("castNm").text();
						var posNm  = $(this).find("posNm").text();
						var deptFullNm = $(this).find("deptFullNm").text();
                    	
						J("#source1").append("<option value=\""+userId+"\">"+userId+" "+userNm+" "+castNm+" "+posNm+" "+"("+deptFullNm+")</option>");
                    });
				}  
        	},  
			error : function(xhr, status, error) {
				alert(error);
			}
    	});
	}
	
