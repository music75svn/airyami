	 * 삭제처리
	 ******************************************/
	function deleteDo() {
		var id = J("#list").getGridParam('selarrrow');
		var ids = J("#list").jqGrid('getDataIDs');
		var rowdata = "";
		var evalDegreeIds = "";
		var evalGrpIds = "";
		var evalUserIds = "";
        
		for (var i = 0; i < ids.length; i++) {
			J.each(id, function (index, value) {
				if (value == ids[i]) {
					rowdata = J("#list").getRowData(ids[i]);
					evalDegreeIds 	+= rowdata.EVAL_DEGREE_ID + '|';
					evalGrpIds 		+= rowdata.EVAL_GRP_ID + '|';
					evalUserIds 	+= rowdata.EVAL_USER_ID + '|';
				}
			});
		}
        
		J("#evalDegreeIds").val(evalDegreeIds);J("#evalGrpIds").val(evalGrpIds);J("#evalUserIds").val(evalUserIds);
		J("#mode").val("DEL");
		J("#year").val(J("#findYear").val());
		J("#formList").attr("action", "$!{context_path}/prs/eval/evalMapping/evalMappingProcess.vw?type=template").submit();
		return false;
	}
	
	/******************************************
	 * 엑셀다운로드
	 ******************************************/
	function excelDownload(){
		J("#yearNm").val(J("#findYear > option:selected").text());
		J("#formList").attr("action", "$!{context_path}/prs/eval/evalMapping/evalMappingListExcel.vw?type=excel").submit();
		return false;
	}
	
	/******************************************
	 * 필수입력 체크
	 ******************************************/
	function saveChk() {
	
		if('$!{CS_HTML.txt2html($confirmYn)}' == 'Y'){
			J.showMsgBox("평가가 확정되어 평가자를 수정할수 없습니다.", null, null);
			return;
		}
		
		if('$!{CS_HTML.txt2html($closeYn)}' == 'Y'){
			J.showMsgBox("평가가 마감되어 평가자를 수정할수 없습니다.", null, null);
			return;
		}
	
		var ids = J("#list").jqGrid('getDataIDs');
		var rowdata = "";
		var weightChk = true;
		var sum = 0;
		var evalGrpNm = "";
		
		for (var i = 1; i < ids.length; i++) {
			rowdata = J("#list").getRowData(i);
			var evalGrpNm = rowdata.EVAL_GRP_NM;
			
			var we1 	=  J("input[name=weight1s]").eq(i-1).val()==""?"0":J("input[name=weight1s]").eq(i-1).val();
			var we2 	=  J("input[name=weight2s]").eq(i-1).val()==""?"0":J("input[name=weight2s]").eq(i-1).val();
			var we3 	=  J("input[name=weight3s]").eq(i-1).val()==""?"0":J("input[name=weight3s]").eq(i-1).val();
			
			sum = parseFloat(we1)+parseFloat(we2)+parseFloat(we3);
			
			sum = Math.round( sum * 10000 ) / 10000;
			
			if(sum != 100){
				weightChk = false;	
				
				J.showMsgBox(evalGrpNm + "의 가중치 합이 100이 되어야 합니다.", null, null);
				return;
				
			}
			
		}
		
		var ids = J("#list").jqGrid('getDataIDs');
		if(ids!=""){
			J.showConfirmBox("저장하시겠습니까?", "saveDo"); //saveDo() 콜백함수 호출
		}
	}
    
	/******************************************
	 * 저장
	 ******************************************/
	function saveDo() {
	
		J("#year").val(J("#findYear").val());
		J("#evalDegreeId").val(J("#findEvalDegreeId").val());
		J("#mode").val("ADD");
		
		var id = J("#list").getGridParam('selarrrow');
		var ids = J("#list").jqGrid('getDataIDs');
		var rowdata = "";
		var evalDegreeIds = "";
		var evalGrpIds = "";
		var evalUser1Ids = "";
		var evalUser2Ids = "";
		var evalUser3Ids = "";
		var weights1 	 = "";
		var weights2 	 = "";
		var weights3 	 = "";
        
		for (var i = 0; i < ids.length; i++) {
			rowdata = J("#list").getRowData(ids[i]);
			var user1Ids =  J("input[name=userOneIds]").eq(i).val()==""?"N":J("input[name=userOneIds]").eq(i).val();
			var user2Ids =  J("input[name=userTwoIds]").eq(i).val()==""?"N":J("input[name=userTwoIds]").eq(i).val();
			var user3Ids =  J("input[name=userThrIds]").eq(i).val()==""?"N":J("input[name=userThrIds]").eq(i).val();
			
			var we1 	=  J("input[name=weight1s]").eq(i).val()==""?"0":J("input[name=weight1s]").eq(i).val();
			var we2 	=  J("input[name=weight2s]").eq(i).val()==""?"0":J("input[name=weight2s]").eq(i).val();
			var we3 	=  J("input[name=weight3s]").eq(i).val()==""?"0":J("input[name=weight3s]").eq(i).val();
			
			evalGrpIds 		+= rowdata.EVAL_GRP_ID + '|';
			evalUser1Ids 	+= user1Ids + '|';
			evalUser2Ids 	+= user2Ids + '|';
			evalUser3Ids 	+= user3Ids + '|';
			weights1 		+= we1 + '|';
			weights2 		+= we2 + '|';
			weights3 		+= we3 + '|';
		}
		J("#evalGrpIds").val(evalGrpIds);
		J("#evalUser1Ids").val(evalUser1Ids);
		J("#evalUser2Ids").val(evalUser2Ids);
		J("#evalUser3Ids").val(evalUser3Ids);
		J("#weights1").val(weights1);
		J("#weights2").val(weights2);
		J("#weights3").val(weights3);
		
		if(ids!=""){
			J("#formList").attr("action", "$!{context_path}/prs/eval/evalMapping/evalMappingProcess.vw?type=template").submit();
		}
	}

//]]>
</script>

