	function goModify(rowId) {
		var ret = J("#list").jqGrid('getRowData',rowId);
		J("#evalDegreeId").val(ret.EVAL_DEGREE_ID);
		J("#evalMembUserId").val(ret.EVAL_MEMB_USER_ID);
		J("#year").val(J("#findYear").val());
		J("#mode").val("MOD");
		J("#formList").attr("action", "$!{context_path}/cbe/eval/evalUser/evalUserModify.vw?type=template").submit();
	}
    
	/******************************************
	 * 삭제처리
	 ******************************************/
	function goSave() {
		var ids = J("#list").jqGrid('getDataIDs');
		
		if(ids.length == 0){
			J.showMsgBox("저장할 데이터가 없습니다.", null, null);
			return;
		}
	
		if(arrayIndex.length == 0){
		
			J.showMsgBox("수정한 데이터가 없습니다.", null, null); 
			return;
		
		}
	
		var evalMembUserIds = "";
		var weightGrpIds = "";
		var rivalGrpIds = "";
		var itemFrsDeptIds = "";
		var itemSecDeptIds = "";
		var itemTeamIds = "";
		var jikgubIds = "";
		var jikgubNms = "";
		var evalMethodGbns = "";
        
		var objId = 0;
		
		for(var idx=0 ; idx<arrayIndex.length ; idx++){
		
			objId = Number(arrayIndex[idx])-1;
			
			evalMembUserIds += J(":input[name=userIds]").eq(objId).val() + '|';
			
			if(J("select[name=weightGrpIds]").eq(objId).val() == ''){
				weightGrpIds 	+= 'none' + '|';
			}else{
				weightGrpIds 	+= J("select[name=weightGrpIds]").eq(objId).val() + '|';
			}
			
			if(J("select[name=rivalGrpIds]").eq(objId).val() == ''){
				rivalGrpIds 	+= 'none' + '|';
			}else{
				rivalGrpIds 	+= J("select[name=rivalGrpIds]").eq(objId).val() + '|';
			}
			
			if(J("select[name=firstDepts]").eq(objId).val() == ''){
				itemFrsDeptIds 	+= 'none' + '|';
			}else{
				itemFrsDeptIds 	+= J("select[name=firstDepts]").eq(objId).val() + '|';
			}
			
			if(J("select[name=secondDepts]").eq(objId).val() == ''){
				itemSecDeptIds 	+= 'none' + '|';
			}else{
				itemSecDeptIds 	+= J("select[name=secondDepts]").eq(objId).val() + '|';
			}
			
			if(J("select[name=teamDepts]").eq(objId).val() == ''){
				itemTeamIds 	+= 'none' + '|';
			}else{
				itemTeamIds 	+= J("select[name=teamDepts]").eq(objId).val() + '|';
			}
			
			if(J("select[name=jikgubs]").eq(objId).val() == ''){
				jikgubIds 	+= 'none' + '|';
			}else{
				jikgubIds 	+= J("select[name=jikgubs]").eq(objId).val() + '|';
			}
			
			if(J("select[name=evalGbns]").eq(objId).val() == ''){
				evalMethodGbns 	+= 'none' + '|';
			}else{
				evalMethodGbns 	+= J("select[name=evalGbns]").eq(objId).val() + '|';
			}
			
		}
        
		J("#evalDegreeId").val(J("#findEvalDegreeId").val());
		J("#evalMembUserIds").val(evalMembUserIds);
		J("#chWeightGrpIds").val(weightGrpIds);
		J("#chRivalGrpIds").val(rivalGrpIds);
		J("#itemFrsDeptIds").val(itemFrsDeptIds);
		J("#itemSecDeptIds").val(itemSecDeptIds);
		J("#itemTeamIds").val(itemTeamIds);
		J("#jikgubIds").val(jikgubIds);
		J("#evalMethodGbns").val(evalMethodGbns);
		
		
		J("#mode").val("MOD");
		J("#year").val(J("#findYear").val());
		J("#formList").attr("action", "$!{context_path}/cbe/eval/evalUser/evalUserProcess.vw?type=template").submit();
		return;
	}
	
