        jQuery("#list").jqGrid('hideCol', "YEAR");
        jQuery("#list").jqGrid('hideCol', "EVAL_MEMB_EMPN_SEQ");
        jQuery("#list").jqGrid('hideCol', "DEPT_CD");
        jQuery("#list").jqGrid('hideCol', "EMPNS");
        jQuery("#list").jqGrid('hideCol', "CAST_TC");
        jQuery("#list").jqGrid('hideCol', "ORG_YN");
        jQuery("#list").setGridWidth($!{jqGrid_r_width});
        
        
		/******************************************
		 * 조회
		 ******************************************/
		J("#formFind").submit(function(event) {
			//submit 하기전 작업추가
		});
        
        var isMngEvalCloseYn = false;
        //평가확정일 경우 
        #if("Y" == "$!{mngEvalYn.MNG_EVAL_YN}")
            isMngEvalCloseYn = true;
        #end
		/******************************************
		 * 간부평가가져오기 버튼 클릭 이벤트
		 ******************************************/
		J("#btnInsaInfo").click(function() {
            if(!isMngEvalCloseYn){
                getInsaInfo();
            } else {
                alertClosing();
            }
			return false;
		});
        
        /******************************************
         * 부서장 정보 버튼 클릭 이벤트
