	/*****************************************************
	 * 지표 실적 상세(팝업)
	 *****************************************************/ 
    function cascadingJS(arg){
	
		if (arg == null) return;
		
		if (arg == "") return;
		
		var arr = arg.split('|');
		if (arr.length < 1) return;
		
		var scDeptId = arr[0];
		var strategyId = arr[1];
		var metricId = arr[2];
		
		var year = J("#findYear").val();
		var mon = J("#findMon").val();
		var analCycle = J(":input:radio[name=findAnalCycle]:checked").val();
		
		var param = "&amp;year=" + year + "&amp;findMon=" + mon + "&amp;metricId=" + metricId + "&amp;analCycle="+analCycle;
		
		//var url = "$!{context_path}/bsc/mon/metricActualDetail/popMetricActualDetailList.vw?type=popup" + param; //지표실적
		var url = "$!{context_path}/bsc/mon/metricDefine/metricDefineList.vw?type=popup" + param; //지표정의서
		openWinByName(url,'popMetricMapping','1250','650','yes');
    }
		
	
//]]>
</script>

