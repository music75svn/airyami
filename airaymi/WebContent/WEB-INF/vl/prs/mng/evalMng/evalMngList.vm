                            {name:'PRS_YN'			,index:'PRS_YN'				,width:85		,align:'center' , formatter:goPrs, sortable:true},
							{name:'ORG_YN'          ,index:'ORG_YN'             ,width:80       ,align:'center' , formatter:goOrg, sortable:true}
