#set($curr_url = $!{context_path} + "/bsc/mon/strategyMatrix/strategyMatrixList.vw")
<script type="text/javascript" language="javascript" >
//<![CDATA[

	var J = jQuery;
	
	J(document).ready(function(){
    
		/******************************************
		 * 조회
		 ******************************************/
		J("#formFind").submit(function(event) {
			//submit 하기전 작업추가
		});
        
		
		/******************************************
		 * fancybox 설정
		 ******************************************/
		J("#anchor").fancybox();    //전략과제선택 팝업
	});


	/*****************************************************
	 * 전략과제선택 팝업
	 *****************************************************/ 
	function popFindStrategy() {
		var findScDeptId = J("#findScDeptId").val();
		var findScDeptNm = J("#findScDeptNm").val();
		var findStrategyId = J("#findStrategyId").val();
		var findStrategyNm = J("#findStrategyNm").val();
		var url = "$!{context_path}/bsc/module/commModule/popStrategyList.vw?findYear=$!{searchMap.txt2html("findYear")}&amp;findMon=$!{searchMap.txt2html("findMon")}&amp;findAnalCycle=$!{searchMap.txt2html("findAnalCycle")}&amp;findScDeptId="+findScDeptId+"&amp;findScDeptNm="+findScDeptNm+"&amp;findStrategyId="+findStrategyId+"&amp;findStrategyNm="+findStrategyNm;
		J("#anchor").attr("href",url);
		J("#anchor").click();
	}
	

	/*****************************************************
	 * 전략과제선택
	 *****************************************************/ 
	function setStrategyId(ret) {
		J("#findScDeptId").val(ret.SC_DEPT_ID);
		J("#findScDeptNm").val(ret.SC_DEPT_NM);
		J("#findStrategyId").val(ret.STRATEGY_ID);
		J("#findStrategyNm").val(ret.STRATEGY_NM_ORIGINAL);
		
        $("#formFind").attr("action", "$!{searchMap.txt2html("context")}/bsc/mon/strategyMatrix/strategyMatrixList.vw?type=template");
        $("#formFind").submit();
	
		J("#popList").setGridParam({url:"$!{searchMap.txt2html("context")}/bsc/module/commModule/popStrategyList_xml.vw?"+J("#formFindPop").serialize()}); 
		J("#popList").trigger("reloadGrid");
		return false;
	}


	/*****************************************************
	 * 전략과제선택 이벤트
	 *****************************************************/ 
    function cascadingJS(arg){
		if (arg == null) return;
		
		var arr = arg.split('|');
		if (arr.length < 1) return;
		
		J("#findScDeptId").val(arr[0]);
		J("#findStrategyId").val(arr[1]);
    	J("#formFind").attr("action", "$!{context_path}/bsc/mon/strategyMetricKpi/strategyMetricKpiList.vw?type=template").submit();
    }

//]]>
</script>

