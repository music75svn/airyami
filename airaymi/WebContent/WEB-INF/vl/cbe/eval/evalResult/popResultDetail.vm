    				<tbody>
						<tr>
							<th rowspan="2" scope="row" style="text-align:center;"><label for="gubun">구분</label></th>
							<th scope="row" style="text-align:center;"><label for="frsTeam">사업소</label></th>
							<th scope="row" style="text-align:center;"><label for="secTeam">2차사업소</label></th>
							<th scope="row" style="text-align:center;"><label for="team">팀</label></th>
							<th rowspan="2" scope="row" style="text-align:center;"><label for="prs">개인</label></th>
							<th rowspan="2" scope="row" style="text-align:center;"><label for="total">계</label></th>
						</tr>
						<tr>
							<th scope="row" style="text-align:center;">$!{CS_HTML.txt2html($detailResult.ITEM_FRS_DEPT_NM)}</th>
							<th scope="row" style="text-align:center;">$!{CS_HTML.txt2html($detailResult.ITEM_SEC_DEPT_NM)}</th>
