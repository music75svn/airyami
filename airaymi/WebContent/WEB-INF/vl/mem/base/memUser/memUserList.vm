	}
	
	/******************************************
	 * 평가자관리 동료평가 재설정
	 ******************************************/
	function getRePeer() {
		J.showConfirmBox("동료평가재설정을 하시면 기존에 입력한 데이터가 삭제됩니다. 진행하시겠습니까?", "doRePeer"); //doRePeer() 콜백함수 호출
	}
	
	function doTargetUserInfo() {
		J("#mode").val("GET");
		J("#formList").attr("action", "$!{context_path}/mem/base/memUser/memUserProcess.vw?type=template").submit();
	}
	
	function doRePeer() {
