	var arrayIndex = new Array(); 
	
	function chkSaveSpot(id){
		
		var chkExists = false; 
		for(var idx=0 ; idx<arrayIndex.length ; idx++){
			if(arrayIndex[idx] == id){
				chkExists = true;
			}
		}
		
		if(!chkExists){
			arrayIndex.push(id);
		}
		
		return;
	}
	
	function checkSelected(val1, val2){
		var selected = ""; 
	
		if(val1 == val2){
			selected = "selected"; 
		}
	
		return selected;
	}
	
	/******************************************
	 * 수정
