	});
	
	/******************************************
     * jqGrid 사원번호
     ******************************************/
    function linkEmpn(cellvalue, options, rowObject) {
    	return '<input type="text" name="evalMembEmpns" value="' + cellvalue + '" class="wp50" readonly="readonly"/>';
    }
    
    /******************************************
     * jqGrid 순번
     ******************************************/
    function linkEmpnSeq(cellvalue, options, rowObject) {
        return '<input type="hidden" name="evalMembEmpnSeqs" value="' + rowObject.EVAL_MEMB_EMPN_SEQ + '" />';
    }
    
	/******************************************
     * jqGrid 간부평가대상여부
     ******************************************/
    function goEval(cellvalue, options, rowObject) {
    	
    	var str = "";
		#*
     	var ids = "evalYn" + options.rowId;
    	
    	if (cellvalue == "Y") {
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="Y" checked="checked"  /> 평가 &nbsp;';
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="N"  /> 미평가 &nbsp;';
    	} else {
    		str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="Y"  /> 평가 &nbsp;';
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="N" checked="checked" /> 미평가 &nbsp;';
    	}
    	*#
		
		var ids = "evalYn" + rowObject.EVAL_MEMB_EMPN + rowObject.EVAL_MEMB_EMPN_SEQ;
    	
    	if (cellvalue == "Y") {
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="Y" checked="checked"  /> 평가 &nbsp;';
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="N"  /> 미평가 &nbsp;';
    	} else {
    		str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="Y"  /> 평가 &nbsp;';
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="N" checked="checked" /> 미평가 &nbsp;';
    	}
		
    	return str;
    	
    }
    
    /******************************************
     * jqGrid 수정페이지 링크
     ******************************************/
    function linkUpdate(cellvalue, options, rowObject) { 
        return '<a href=javascript:goModify(' + options.rowId + ')>' + cellvalue + '<//a>';
    }
    
    /*****************************************************
	 * 간부평가대상여부 변경 이벤트(미평가)
	 *****************************************************/ 
	function changeYn(obj) {
		
		 var str = obj.id.replace("evalYn","");
		 
		 var evalYn = J(":input:radio[name=evalYn" + str + "]:checked").val();
		 
		 if (evalYn =="Y") {
		 
		 	J(":input:radio[name=prsYn" + str + "]").attr("disabled", false);
			J(":input:radio[name=orgYn" + str + "]").attr("disabled", false);
		 	
		 } else {
		 	
		 	J(":input:radio[name=prsYn" + str + "]").attr("disabled", true);
			J(":input:radio[name=prsYn" + str + "]").attr("checked", true);
			J(":input:radio[name=orgYn" + str + "]").attr("disabled", true);
			J(":input:radio[name=orgYn" + str + "]").attr("checked", true);
		 }
		 
	}
	
    /*****************************************************
	 * 화면초기셋팅
	 *****************************************************/ 
	function init() {
		
		var ids        = J("#list").jqGrid('getDataIDs');
    	  
		for(i=0; i<ids.length ; i++) {
	  
			var evalYn = J(":input:radio[name=evalYn" + i + "]:checked").val();
			//alert(evalYn);
	  		if (evalYn =="N") {
	  	
	    	  	J(":input:radio[name$=prsYn" + i + "]").attr("disabled", true);
				J(":input:radio[name$=orgYn" + i + "]").attr("disabled", true);
			}
		}
	}
    
	/******************************************
     * jqGrid 개인업적평가여부
     ******************************************/
    function goPrs(cellvalue, options, rowObject) {
    	
    	var str = "";
    	##var ids = "prsYn" + options.rowId;
		var ids = "prsYn" + rowObject.EVAL_MEMB_EMPN + rowObject.EVAL_MEMB_EMPN_SEQ;
    	
    	if (cellvalue == "Y") {
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="Y" checked="checked" /> 대상 &nbsp;';
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="N" /> 제외 &nbsp;';
    	} else {
    		str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="Y" /> 대상 &nbsp;';
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="N" checked="checked" /> 제외 &nbsp;';
    	}
    	
    	return str;
    	
    }
    
	/******************************************
     * jqGrid 조직관리역량평가여부
     ******************************************/
    function goOrg(cellvalue, options, rowObject) {
    	
    	var str = "";
    	##var ids = "orgYn" + options.rowId;
		var ids = "orgYn" + rowObject.EVAL_MEMB_EMPN + rowObject.EVAL_MEMB_EMPN_SEQ;
    	
    	if (cellvalue == "Y") {
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="Y" checked="checked" /> 평가 &nbsp;';
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="N" /> 미평가 &nbsp;';
    	} else {
    		str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="Y" /> 평가 &nbsp;';
	    	str += '<input type="radio" id="' + ids + '" name="' + ids + '" value="N" checked="checked" /> 미평가 &nbsp;';
    	}
    	
    	return str;
    	
    }
    
	/******************************************
	 * 그리드 ajax 조회
	 ******************************************/
	function goGridSearch() {
		J("#list").setGridParam({url:"$!{context_path}/prs/mng/evalMng/evalMngList_xml.vw?"+J("#formFind").serialize(), datatype:"json"}); 
		J("#list").trigger("reloadGrid");
	}
	
	/******************************************
	 * 조직선택 클릭이벤트
	 ******************************************/
	function goAction(deptCd, deptNm) {
		setDeptInfo(deptCd, deptNm);
	}
	
	/*****************************************************
	 * 조직선택 설정
	 *****************************************************/ 
	function setDeptInfo(deptCd, deptNm) {
		
		// 조직트리에서 조직 선택 시 Ajax로 데이터그리드만 다시 조회하므로 formList의 searchMap.makeHiddenFind() 가 다시 생성되지 않음.
		// formList가 submit되는 경우(신규, 수정, 삭제) 조직트리에서 선택된 조직의 정보를  유지시켜주기 위해 설정.
		J("#formList input[name=findDeptCd]").val(deptCd);
		J("#formList input[name=findDeptNm]").val(J(deptNm).text());
		
		//var findCastTc = J("input:radio[name=findCastTc]:checked").val();
		J('input:radio[name=findCastTc]:input[value=""]').attr("checked", true);
		
	
			// formFind의 변수들 설정.
			J("#findDeptCd").val(deptCd);			// 조직Tree에서 선택한 조직코드를 설정.
			J("#findDeptNm").val(J(deptNm).text());	// 조직Tree에서 선택한 조직명을 설정.(신규 등록시 사용함.)
			
			goGridSearch();
			
	}
	
	/******************************************
	 * 간부평가 대상자 불러오기
	 ******************************************/
	function getInsaInfo() {
		J.showConfirmBox("간부평가 대상자정보를 가져오시겠습니까?", "getInsaInfoDo"); //getInsaInfoDo() 콜백함수 호출
	}
	
	function getInsaInfoDo() {
		J("#mode").val("GET");
        J("#year").val(J("#findYear").val());
		J("#formList").attr("action", "$!{context_path}/prs/mng/evalMng/evalMngProcess.vw?type=template").submit();
	}
    
    /******************************************
     * 부서장정보 불러오기
     ******************************************/
    function getManagerInfo() {
        J.showConfirmBox("부서장정보를 업데이트 하시겠습니까?", "getManagerInfoDo"); //getInsaInfoDo() 콜백함수 호출
    }
    
    function getManagerInfoDo() {
        J("#mode").val("MANAGER");
        J("#year").val(J("#findYear").val());
        J("#formList").attr("action", "$!{context_path}/prs/mng/evalMng/evalMngProcess.vw?type=template").submit();
    }
    
    /******************************************
     * 엑셀다운로드
     ******************************************/
    function excelDownload(){
        J("#castTcNm").val(J(":input:radio[name=findCastTc]:checked").attr("alt"));
        J("#evalGrpNm").val(J("#findEvalGrpId > option:selected").text());
        J("#userNm").val(J("#findKorNm").val());
        J("#formList").attr("action", "$!{context_path}/prs/mng/evalMng/evalMngListExcel.vw?type=excel").submit();
        return false;
    }
	
	/******************************************
     * 필수입력 체크
     ******************************************/
    function saveChk() {
		var ids	= J("#list").jqGrid('getDataIDs');
		
		J.showConfirmBox("저장하시겠습니까?", "saveDo"); //saveDo() 콜백함수 호출
		
    }

    /******************************************
     * 저장
     ******************************************/
    function saveDo() {	
    	
    	  var ids        = J("#list").jqGrid('getDataIDs');
    	  
   #* 
		  for(i=1; i<=ids.length ; i++) {
    	  	J(":input:radio[name$=prsYn" + i + "]").attr("disabled", false);
			J(":input:radio[name$=orgYn" + i + "]").attr("disabled", false);
    	  }
    *#
		
        J("#mode").val("SAVE");
        J("#year").val(J("#findYear").val());
        J("#formList").attr("action", "$!{context_path}/prs/mng/evalMng/evalMngProcess.vw?type=template").submit();
                
        return false;

    }
    
    /******************************************
     * 대상자 추가
     ******************************************/
    function goInsert() {
        
        J("#year").val(J("#findYear").val());
        J("#mode").val("ADD");
        J("#formList").attr("action", "$!{context_path}/prs/mng/evalMng/evalMngModify.vw?type=template").submit();
    }
    
    /******************************************
     * 수정
     ******************************************/
    function goModify(rowId) {
        var ret = J("#list").jqGrid('getRowData',rowId);
        
        J("#year").val(ret.YEAR);
        J("#evalMembEmpn").val(ret.EVAL_MEMB_EMPN);
        J("#evalMembEmpnSeq").val(ret.EVAL_MEMB_EMPN_SEQ);
        J("#mode").val("MOD");
        J("#formList").attr("action", "$!{context_path}/prs/mng/evalMng/evalMngModify.vw?type=template").submit();
    }
    
    /******************************************
     * 삭제
     ******************************************/
    function goDelete() {
        if(!J.gridSelectChk("list")){
            J.showMsgBox("삭제할 데이터를 선택하십시요", null, null); 
        } else {
            J.showConfirmBox("<font color='red' style='font-weight:bold;'>성과계획서 및 평가결과</font>가 있을 경우 삭제됩니다.\n삭제하시겠습니까?", "deleteDo"); //deleteDo() 콜백함수 호출
        }
    }
    
    /******************************************
     * 삭제처리
     ******************************************/
    function deleteDo() {
        var id = J("#list").getGridParam('selarrrow');
        var ids = J("#list").jqGrid('getDataIDs');
        var rowdata = "";
        var year = "";
        var evalMembEmpn = "";
        var evalMembEmpnSeq = "";
        
        for (var i = 0; i < ids.length; i++) {
            J.each(id, function (index, value) {
                if (value == ids[i]) {
                    rowdata = J("#list").getRowData(ids[i]);
                    year = rowdata.YEAR;
                    evalMembEmpn += rowdata.EVAL_MEMB_EMPN + '|';
                    evalMembEmpnSeq += rowdata.EVAL_MEMB_EMPN_SEQ + '|';
                }
            });
        }
        
        J("#mode").val("DEL");
        J("#year").val(year);
        J("#evalMembEmpn").val(evalMembEmpn);
        J("#evalMembEmpnSeq").val(evalMembEmpnSeq);
        J("#formList").attr("action", "$!{context_path}/prs/mng/evalMng/evalMngProcess.vw?type=template").submit();
        return false;
    }
    
    /******************************************
     * 라디오버튼 변경시
     ******************************************/
    function reNew() {
    	
    	var findCastTc1 = J("input:radio[name=findCastTc]:checked").val();
		J('input:radio[name=findCastTc]:input[value=findCastTc1]').attr("checked", true);
    		
		J("#formFind input[name=findDeptCd]").val("0000");
		
        J("#formFind").attr("action", "$!{context_path}/prs/mng/evalMng/evalMngList.vw?type=template").submit();
                
        return false;

    }
    /******************************************
     * 평가확정알림
     ******************************************/
    function alertClosing(){
        alert("간부개인업적평가가 확정되었습니다.\n 확인하여 주십시오.");
        return false;
    }
	
//]]>
</script>

