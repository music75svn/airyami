<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="ko">

#set($curr_url = $!{context_path} + "/bsc/popup/productivityBranch/productivityBranchList.vw")

<head>
	#parse("/WEB-INF/vl/common/template/head.vm")
<script type="text/javascript" language="javascript" >
//<![CDATA[

	var J = jQuery;
	
	J(document).ready(function(){
		/******************************************
		 * 데이터 그리드 출력
		 ******************************************/
		var str = J('#formFind').serialize();
		
	#if ($searchMap.get("findItemCd") == "S010D001")
		jQuery("#list").jqGrid({
			url          :"$!{context_path}/bsc/popup/productivityBranch/productivityBranchList_xml.vw" + "?" + str,
			mtype        :"POST",
			datatype     :"json",
			jsonReader   : {   
    						page   : "page",
    						total  : "total",
    						root   : "rows", 
    						records: function(obj){return obj.length;},
    						repeatitems: false, 
    						id     : "id"
    				       },
			height       : $!{jqGrid_height},
			width        : $!{jqGrid_width},
			colNames     :['년도', '월', '부서코드', '부서명', '업무구분', '검사구분', '가스법구분', '대분류', '중분류', '소분류', '신규검사수', '재검수', '배관검사길이', '적용건수', '상대값', '상대값의합'],
			colModel     :[
							{name:'YEAR'  		 		,index:'YEAR'  		   	 	,width:30   ,align:'center' },
                            {name:'MON'      			,index:'MON'      		 	,width:20   ,align:'center' },
                            {name:'DEPT_CD' 			,index:'DEPT_CD'			,width:50   ,align:'center' },
                            {name:'DEPT_NM'    			,index:'DEPT_NM'   			,width:40   ,align:'center' },
                            {name:'ROLE_TC_NM'     		,index:'ROLE_TC_NM'			,width:40   ,align:'center' },
                            {name:'INSP_KIND_TC_NM'     ,index:'INSP_KIND_TC_NM'	,width:40   ,align:'center' },
                            {name:'LAW_TC_NM'     		,index:'LAW_TC_NM'			,width:40   ,align:'center' },
                            {name:'STIC_LAG_TC_NM'     	,index:'STIC_LAG_TC_NM'		,width:40   ,align:'center' },
                            {name:'STIC_MID_TC_NM'     	,index:'STIC_MID_TC_NM'		,width:40   ,align:'center' },
                            {name:'STIC_SML_TC_NM'     	,index:'STIC_SML_TC_NM'		,width:40   ,align:'center' },
                            {name:'INSP_QTY' 			,index:'INSP_QTY' 			,width:60   ,align:'center' },
                            {name:'RE_INSP_QTY' 		,index:'RE_INSP_QTY' 		,width:60   ,align:'center' },
                            {name:'LENG' 				,index:'LENG' 				,width:60   ,align:'center' },
                            {name:'TOTAL' 				,index:'TOTAL' 				,width:60   ,align:'center' },
                            {name:'RLTV_VAL' 			,index:'RLTV_VAL' 			,width:60   ,align:'center' },
                            {name:'TOTAL_RLTV_VAL' 		,index:'TOTAL_RLTV_VAL' 	,width:60   ,align:'center' }	
							],
			rowNum       : $!{jqGrid_rownum},
			autowidth    : false, 
			viewrecords  : true,
			loadonce     : true,
			multiselect  : false,
			cmTemplate	 : {sortable:false},
			cellEdit     : true,
			caption:"시스템 연계데이터 조회"
		}).jqGrid('hideCol',"DEPT_CD").setGridWidth($!{jqGrid_width});
	#else
		jQuery("#list").jqGrid({
			url          :"$!{context_path}/bsc/popup/productivityBranch/productivityBranchList_xml.vw" + "?" + str,
			mtype        :"POST",
			datatype     :"json",
			jsonReader   : {   
    						page   : "page",
    						total  : "total",
    						root   : "rows", 
    						records: function(obj){return obj.length;},
    						repeatitems: false, 
    						id     : "id"
    				       },
			height       : $!{jqGrid_height},
			width        : $!{jqGrid_width},
			colNames     :['년도','월','부서명','직급', '사번', '성명', '직위','기술인력수'],
			colModel     :[
							{name:'YEAR'           		,index:'YEAR'      	 		,width:50   	,align:'center' ,cellattr: attrSetting}, 
							{name:'MON'           		,index:'MON'      	 		,width:50   	,align:'center' ,cellattr: attrSetting},
                            {name:'DEPT_NM'       		,index:'DEPT_NM'         	,width:100   	,align:'center' ,cellattr: attrSetting},
                            {name:'CAST_TC_NM'       	,index:'CAST_TC_NM'         ,width:100   	,align:'center' },
                            {name:'EMPN'				,index:'EMPN'				,width:100    	,align:'center' },	
							{name:'KOR_NM'				,index:'KOR_NM'				,width:100    	,align:'center' },	
							{name:'POS_TC_NM'			,index:'POS_TC_NM'			,width:100    	,align:'center' },	
							{name:'TOTAL_CNT'			,index:'TOTAL_CNT'			,width:80    	,align:'center' ,cellattr: attrSetting}	
							],
			rowNum       : $!{jqGrid_rownum},
			autowidth    : false, 
			viewrecords  : true,
			loadonce     : true,
			multiselect  : false,
			cmTemplate	 : {sortable:false},
			cellEdit     : true,
			caption:"시스템 연계데이터 조회"
		});
	#end

		/******************************************
		 * 엑셀다운로드 버튼 클릭 이벤트
		 ******************************************/
		J("#btnExcel").click(function() {
			J("#yearNm").val(J("#findYear > option:selected").text());
			J("#monNm").val(J("#findMon > option:selected").text());
			J("#deptNm").val(J("#findDeptId > option:selected").text());
			J("#itemNm").val(J("#findItemCd > option:selected").text());
			J("#formFind").attr("action", "$!{context_path}/bsc/popup/productivityBranch/excel.vw?type=excel").submit();
			J("#formFind").attr("action", "$!{context_path}/bsc/popup/productivityBranch/productivityBranchList.vw?type=popup");
			return false;
		});
	});
	

	/*****************************************
	 * 업무구분 Rowspan 설정
	 ******************************************/
	function attrSetting(rowId, val, rowObject, cm) {
		var result;
		var index = rowObject.TOTAL_INDEX;
		var rowSpan = rowObject.TOTAL_CNT;

		if(index == 1) {
			result = ' rowspan="' + rowSpan + '"';
		} else {
			result = ' style="display:none"';
		}

		return result;
	}
//]]>
</script>
</head>
<body>
<div id="Table">
	<div id="Table_blockContainer">
		<h1 class="none">시스템 연계데이터 조회</h1>
		<div class="tableTop">
			
    		<!-- search S -->
			<form id="formFind" name="formFind" method="post" action="$!{context_path}/bsc/popup/productivityBranch/productivityBranchList.vw?type=popup">
				<input type="hidden"	id="findHighPgmId"	name="findHighPgmId"	value="$!{searchMap.txt2html("findHighPgmId")}" />
				<input type="hidden" 	id="findSubPgmId"	name="findSubPgmId"		value="$!{searchMap.txt2html("findSubPgmId")}" />
				<input type="hidden"    id="findPgmId"		name="findPgmId"		value="$!{searchMap.txt2html("findPgmId")}" />
				<input type="hidden"    id="findMetricId"	name="findMetricId"		value="$!{searchMap.txt2html("findMetricId")}" />
				<input type="hidden" id="mode"       name="mode" />
    			<input type="hidden" id="yearNm"	name="yearNm" />
    			<input type="hidden" id="monNm"		name="monNm" />
    			<input type="hidden" id="deptNm"	name="deptNm" />
    			<input type="hidden" id="itemNm"	name="itemNm" />
				<div class="clear h20"></div>
				<div class="searchBox">
    				<div><div><div>
    				<table summary="시스템 연계데이터 조회검색 조건">
						<caption class="none">시스템 연계데이터 조회 검색 조건</caption> 
						<thead><tr><th></th></tr></thead>
        				<tbody>
            				<tr>
								<td class="searchBox_tit w280">
            						<span><label for="findYear">평가년월</label></span>
            						<span class="searchBar">
            							<select id="findYear" name="findYear" class="select w80">
            								$!CS_CODE.getCodeOption("017", $!{searchMap.txt2html("findYear")})
            							</select>
            							<select id="findMon" name="findMon" class="select w70">
            								##<option value="">전체</option>
            								$!CS_CODE.getCodeOption("024", $!{searchMap.txt2html("findMon")})
            							</select>
            						</span>
            					</td>
            					<td class="searchBox_tit w270">
                					<span><label for="findDeptId">부서</label></span>
									<span class="searchBar">
										<select id="findDeptId" name="findDeptId" class="select w200">
                                            ##<option value="">전체</option>
											#foreach($list in $deptInfo)
												<option value="$!{CS_HTML.txt2html($list.DEPT_ID)}" $searchMap.getSelected("findDeptId", "$!{CS_HTML.txt2html($list.DEPT_ID)}")>$!{CS_HTML.txt2html($list.DEPT_NM)}</option>
											#end
            							</select>
									</span>
                                </td>
								<td class="searchBox_tit w270">
                					<span><label for="findItem">항목</label></span>
									<span class="searchBar">
										<select id="findItemCd" name="findItemCd" class="select w200">
											#foreach($item in $itemInfo)
												<option value="$!{CS_HTML.txt2html($item.ITEM_CD)}" $searchMap.getSelected("findItemCd", "$!{CS_HTML.txt2html($item.ITEM_CD)}")>$!{CS_HTML.txt2html($item.CAL_TYPE_COL_NM)}</option>
											#end
            							</select>
									</span>
                                </td>
            				</tr>
        				</tbody>
        				<div class="blockButton " style="position:absolute;right:10px; width:150px;float:right; align:middle;padding: 0 10px 0 0;" >
                    		<input id="btnExcel" type="button" align="middle" value="엑셀다운로드", alt="엑셀다운로드" />
                    	</div>
    				</table>
                    <input type="image" class="btnSearch" src="$!{img_path}/common/btn_search.gif" alt="검색" style="position:absolute;right:120px;"/> 
                    </div></div></div>
    	        </div>
    		</form>
    		<!-- search E -->
			<div class="clear"></div>
		</div>
		
		<form id="formList" name="formList" method="post" action="">
			
			$!{searchMap.makeHiddenFind()}
			<div id="splitterBox">
				<div>
					<!-- 데이터그리드 Start -->
					<div class="blockGridListTable">
						<table id="list" summary="시스템 연계데이터 조회 조회결과">
							<caption class="none">시스템 연계데이터 조회 조회</caption>
							<thead><tr><th></th></tr></thead>
							<tbody><tr><td></td></tr></tbody>
						</table>
						<div id="pager"></div>
					</div>
					<!--// 데이터그리드 End -->
				</div>
			</div>
		</form>
		
	</div>
	
	<div class="clear"></div>
</div>
</body>
</html>