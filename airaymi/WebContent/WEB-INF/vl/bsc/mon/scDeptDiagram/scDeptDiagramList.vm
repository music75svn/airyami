		<form id="formList" name="formList" method="post" action="$!{context_path}/bsc/mon/scDeptDiagram/scDeptDiagramProcess.vw?type=template">
			<input type="hidden" id="mode"       		name="mode" />
			<input type="hidden" id="year"		 		name="year" />
			<input type="hidden" id="findStrategyId"	name="findStrategyId" />
			<input type="hidden" id="findStrategyNm"	name="findStrategyNm" />
			<input type="hidden" id="findMetricId"		name="findMetricId" />
			<input type="hidden" id="findMetricNm"		name="findMetricNm" />
			$!{searchMap.makeHiddenFind()}
			
		<div id="total" style="width:100%; text-align:center;" >
			<div id="scDeptDiagramBox" style="width:80%; float:left; text-align:center;">
				<div style="width:100%; text-align:center;">
                    <div style="width:100%; text-align:left;">
                    	<div id="chartContainer"></div>
						<script type="text/javascript" language="javascript">
						//<![CDATA[
							var scDiagramGubun = J("#findDiagramGubun").val();
							
							if(scDiagramGubun == "01") {
								scDiagramGubun = "D000001";
							} else if(scDiagramGubun == "02") {
								scDiagramGubun = "D000002";
							} else if(scDiagramGubun == "03") {
