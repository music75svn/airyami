	function add() {
			
		if( J("#source option").length == 0) {
			J.showMsgBox("이동할 데이터가 없습니다.", null, null);
    		return;
    	}
    
    	if(J("#source option:selected").length == 0) {
			J.showMsgBox("이동할 데이터를 선택하십시오.", null, null);
    		return;
    	}
		
		J("#source option:selected").each(function() {  
			var temp_text = J(this).text();
			var temp_value = J(this).val();
			var temp_id = J(this).attr('id');
			var temp_target = J("#target option").length;
			
		    if(temp_target >= 6){
				J.showMsgBox("6명까지 선택 할 수 있습니다.", null, null);
				return false;				
			}
			
			if(sourceSelectedItem(temp_value) == false) {
				J("#target").append("<option id=\""+temp_id+"\" value=\""+temp_value+"\">"+temp_text+"</option>");
			#foreach($userList in $userList)
				if('$!{CS_HTML.txt2html($userList.ASSESSOR_EMPN)}' == temp_value){
        			  if(temp_id == "0"){
    					J("#delData").val("del");
        				return false;
    				   }else{
        				J("#delData").val("safe");
    				   }
				 }else{
        			  if(temp_id != "0"){
        				J("#delData").val("safe");
        				return false;
    				   }
				 }
		   #end
			} else {
				J.showMsgBox("선택 하신 Data는 이미 선택 되어 있습니다.", null, null);
				return false;
			}
			J(this).remove();
        });
		
    }

    function del() {
		if( J("#target option").length == 0) {
			J.showMsgBox("이동할 데이터가 없습니다.", null, null);
    		return;
    	}
    
    	if(J("#target option:selected").length == 0) {
			J.showMsgBox("이동할 데이터를 선택하십시오.", null, null);
    		return;
    	}

		J("#target option:selected").each(function() {  
			var temp_text = J(this).text();
			var temp_value = J(this).val();
			var temp_id = J(this).attr('id');
			if(targetSelectedItem(temp_value) == false) {
				if(temp_id != "0"){
        			J.showMsgBox("기존 평가점수가 삭제 됩니다.", null, null);
        			J("#delData").val("del");
				}
				J("#source").append("<option id=\""+temp_id+"\" value=\""+temp_value+"\">"+temp_text+"</option>");
			}else{
				if(temp_id != "0"){
        			J("#delData").val("del");
