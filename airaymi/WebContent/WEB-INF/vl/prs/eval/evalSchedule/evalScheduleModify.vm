<script type="text/javascript" language="javascript" >
//<![CDATA[

	var J = jQuery;

	J(document).ready(function(){
	
	
		J("input[name$=StartDt]").datepicker({
			dateFormat: 'yy.mm.dd'
		});
		J("input[name$=EndDt]").datepicker({
			dateFormat: 'yy.mm.dd'
		});

		/******************************************
		 * 취소버튼 (돌아가기)
		 ******************************************/
		J("#btnBack").click(function() {
			J("#form1").attr("action", "$!{context_path}/prs/eval/evalSchedule/evalScheduleList.vw?type=template").submit();
		});
        
		/******************************************
		 * 저장버튼 이벤트
		 ******************************************/
		J("#btnInsert").click(function(e) {
			e.preventDefault();
			saveChk();
		});

		/******************************************
		 * 저장버튼 이중 클릭 방지
		 ******************************************/
		J("form").submit(function(){
			J("#btnInsert").unbind();
		});

	});

	/******************************************
	 * 필수입력 체크
	 ******************************************/
	function saveChk() {
		var isContinue = true;
        
		J("input[name$=StartDt]").each(function(){
			
			var idNm = J(this).attr("id");
			var labelText = J("label[for="+idNm+"]").text();
            
			if(!J.checkLength(idNm, labelText,1, 10)) {
				isContinue = false;
        		return;
    		}
			return isContinue; 
		});
		if( isContinue ){ }else {	return ;	}
		
		J("input[name$=EndDt]").each(function(){
			var idNm = J(this).attr("id");
			var labelText = J("label[for="+idNm+"]").text();
			if(!J.checkLength(idNm, labelText,1, 10)) {
				isContinue = false;
        		return;
    		}
			return isContinue; 
		});
		if( isContinue ){ }else {	return ;	}
	
		
		J("input[name$=StartDt]").each(function(){
			var idNm = J(this).attr("id");
			var preText = idNm.replace("StartDt","" );
			var idNm2 = preText + "EndDt";
			
			var startDt = J(this).val();
			var endDt = J("#" + preText + "EndDt").val();
			
			if( startDt > endDt ) {
				var startLabelText = J("label[for="+idNm+"]").text();
				var endLabelText = J("label[for="+idNm2+"]").text();
				J.showMsgBox(endLabelText + "가  " + startLabelText + " 보다 빠를 수 없습니다.", null, null);
				isContinue = false;
        		return;
    		}
			
			return isContinue; 
		});
		if( isContinue ){ }else {	return ;	}
		
		J.showConfirmBox("저장하시겠습니까?", "saveDo"); //saveDo() 콜백함수 호출
	}
    
	/******************************************
	 * 저장
	 ******************************************/
	function saveDo() {
		J("input[name$=StartDt]").each(function(){
			J(this).val((J(this).val().replace(/[.]/g, "")));
		});
		J("input[name$=EndDt]").each(function(){
			J(this).val((J(this).val().replace(/[.]/g, "")));
		});
	
		J("#form1").attr("action", "$!{context_path}/prs/eval/evalSchedule/evalScheduleProcess.vw?type=template").submit();
	}

//]]>
</script>    
 
