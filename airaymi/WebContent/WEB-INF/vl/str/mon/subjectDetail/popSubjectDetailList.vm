#*************************************************************************
* CLASS 명      : popSubjectDetailList
* 작 업 자      : 하윤식
* 작 업 일      : 2012년 11월 01일 
* 기    능      : 세부과제 정의서(팝업)
* ---------------------------- 변 경 이 력 --------------------------------
* 번호    작 업 자     작   업   일       변 경 내 용           비  고
* ----  --------  -----------------  -------------------------  -----------
*  1     하윤식      2012년 11월 01일     최 초 작 업
**************************************************************************# 
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="ko">

#set($main_url = $!{searchMap.txt2html("mainUrl")})
#set($curr_url = $!{context_path} + "/str/mon/metricMapping/popSubjectDetailList.vw")

<head>
	#parse("/WEB-INF/vl/common/template/head.vm")
    <script type="text/javascript" language="javascript" >
	
	var J = jQuery;
	
	J(document).ready(function(){
	
		/******************************************
		 * 조회
		 ******************************************/
		J("#formFind").submit(function(event) {
			return false;
		});
		
		/******************************************
		 * 닫기버튼 (창닫기)
		 ******************************************/
		J("#btnClose").click(function() {
			self.close();
		});
		 
	});
	
    </script>
</head>
<body>
	
<div>
	<div class="pop_blockContainer">
		<form id="formFind" name="formFind" method="post" action="$!{context_path}/str/mon/metricMapping/popMetricMappingList.vw?type=pop">
			<input type="hidden"	id="findHighPgmId"			name="findHighPgmId"		value="$!{searchMap.txt2html("findHighPgmId")}" />
			<input type="hidden" 	id="findSubPgmId"			name="findSubPgmId"			value="$!{searchMap.txt2html("findSubPgmId")}" />
			<input type="hidden"    id="findPgmId"				name="findPgmId"			value="$!{searchMap.txt2html("findPgmId")}" />
			<input type="hidden"    id="findSubjectId"          name="findSubjectId"        value="$!{searchMap.txt2html("findSubjectId")}" />
			

		</form>
		
		<div class="clear h10"></div>
		<div>
		<table width="100%" border="0" cellspacing="0" cellpadding="0">
		<tr>
			<td>
				<table width="100%" border="0" cellpadding="0" cellspacing="0" class="popup_tit_c">
				<tr>
					<td class="popup_tit_l"></td>
					<td class="popup_tit_r">세부과제 정의서</td>
				</tr>
				</table>
			</td>
		</tr>
		</table>	
		</div>
		
		<div class="clear h10"></div>
		
		
		<div class="blockDetail">
			<table summary="세부과제 정의서">
				<caption class="none">세부과제 정의서</caption>
				<colgroup>
					<col width="15%" />
					<col width="35%" />
					<col width="15%" />
					<col width="35%" />
				</colgroup>
				<tbody>
					<tr>
						<th scope="row">$!{YEAR}</th>
						<td colspan="3">$!{searchMap.txt2html("findYear")} 년</td>
					</tr>
					<tr>
						<th scope="row">전략과제</th>
						<td>$!{CS_HTML.txt2html($detail.STRAT_SUBJECT_NM)}</td>
						<th scope="row">전략방향</th>
						<td>$!{CS_HTML.txt2html($detail.STRAT_DIRECT_NM)}</td>
					</tr>
					<tr>
						<th scope="row">세부과제</th>
						<td colspan="3">$!{CS_HTML.txt2html($detail.SUBJECT_NM)}</td>
					</tr>
					<tr>
						<th scope="row">세부과제목표</th>
						<td colspan="3">$!{CS_HTML.txt2html($detail.SUBJECT_TARGET)}</td>
					</tr>
					<tr>
						<th scope="row">담당자</th>
						<td>$!{CS_HTML.txt2html($detail.CHARGE_USER_NM)}</td>
						<th scope="row">담당부서</th>
						<td>$!{CS_HTML.txt2html($detail.CHARGE_DEPT_NM)}</td>
					</tr>
					<tr>
						<th scope="row">$!{CS_HTML.txt2html($detail.YEAR)}년도 실행계획</th>
						<td colspan="3">
							<textarea id="yearActualPlan" name="yearActualPlan"  class="tabletext" title="$!{CS_HTML.txt2html($detail.YEAR)}년도 실행계획" cols="10" rows="6" readonly="readonly">$!{CS_HTML.txt2html($detail.YEAR_ACTUAL_PLAN)}</textarea>
						</td>
					</tr>
					<tr>
						<th scope="row">$!{CS_HTML.txt2html($detail.Y_1)} ~ $!{CS_HTML.txt2html($detail.Y_2)}년도 실행계획</th>
						<td colspan="3">
							<textarea id="multyYearActualPlan" name="multyYearActualPlan"  class="tabletext" cols="10" rows="6" readonly="readonly">$!{CS_HTML.txt2html($detail.MULTY_YEAR_ACTUAL_PLAN)}</textarea>
						</td>
					</tr>
					<tr>
						<th scope="row">첨부파일</th>
						<td colspan="3">
							#foreach($fileList in $fileList)
        						$!{CS_HTML.makeFileIconFileName($fileList.get("ATTACH_FILE_SUFFIX"), $fileList.get("ATTACH_FILE_NM"), $fileList.get("ATTACH_FILE_PATH"))} <br /> 
							#end
						</td>
					</tr>
					<tr>
						<th scope="row">정렬순서</th>
						<td colspan="3">$!{CS_HTML.txt2html($detail.SORT_ORDER)}</td>
					</tr>
				</tbody>
			</table>
		</div>
		
		<div class="blockButton"> 
			<ul>
				<li><input type="button" id="btnClose" value="닫기"/></li>
			</ul>
		</div>
	</div>
	<div class="clear"></div>
</div>

<div id="dialog" title="확인"><p>&nbsp;</p></div>    <!-- 다이어로그 -->

</body>
</html>
