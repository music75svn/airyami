	/******************************************
55	 * 목표입력자 선택시 검색   flag ->  0:담당자존재     1:미지정      2:전체
	 ******************************************/
	function goSearch() {
		if( J("#findActualType").val() == "input" ) {
			if( J("#findInsertUserId").val() == '' ) {
				J("#flag").val("1");
				J("#findTargetStatusId").val("01");
			}else {
				J("#flag").val("0");
				J("#findTargetStatusId").append("<option value='ALLSTATUS'>전체</option>");
				J("#findTargetStatusId").val("ALLSTATUS");
			}
		}else {
			if( J("#findApproveUserId").val() == '' ) {
				J("#flag").val("1");
				J("#findTargetStatusId").val("01");
			}else if( J("#findApproveUserId").val() == "ALL" ) {
				J("#flag").val("2");
				J("#findTargetStatusId").append("<option value='ALLSTATUS'>전체</option>");
				J("#findTargetStatusId").val("ALLSTATUS");
			}else {
				J("#flag").val("0");
				J("#findTargetStatusId").append("<option value='ALLSTATUS'>전체</option>");
				J("#findTargetStatusId").val("ALLSTATUS");
			}
		}

		J("#formFind").attr("action", "$!{context_path}/bsc/tam/targetMng/targetMngList.vw?type=template").submit();

	}

