<script type="text/javascript" language="javascript" >
//<![CDATA[

	var J = jQuery;

	J(document).ready(function(){

		/******************************************
		 * 조회
		 ******************************************/
		J("#formFind").submit(function(event) {
			//submit 하기전 작업추가
		});

		/******************************************
		 * fancy Box 팝업호출
		 ******************************************/
		J("#anchor").fancybox();

		goStrategy('', '');
	});
	
	var deptChart2;
	var deptChart3;
	
	function goStrategy(manId) {
		FusionCharts.setCurrentRenderer(render);
		if(!FusionCharts("chartId2")){
			deptChart2 = new FusionCharts("$!{chart_path}/MSColumn3D.swf", "chartId2", "1135", "200", "0", "1" );
		}
		deptChart2.configure("ChartNoDataText", " ");
		deptChart2.setXMLUrl("$!{context_path}/man/mon/contribute/strategyChart_xml.vw?year=$!{searchMap.txt2html("findYear")}&manId=" + manId);
		deptChart2.render("barStrategy");
		
		FusionCharts.setCurrentRenderer(render);
		if(!FusionCharts("chartId3")){
			deptChart3 = new FusionCharts("$!{chart_path}/MSColumn3D.swf", "chartId3", "1135", "200", "0", "1" );
		}
		deptChart3.configure("ChartNoDataText", " ");
		deptChart3.setXMLUrl("$!{context_path}/man/mon/contribute/csfChart_xml.vw?year=$!{searchMap.txt2html("findYear")}&manId=" + manId + "&stratId=");
		deptChart3.render("barCsf");
	}
	
	function goCsf(manId, stratId) {
		FusionCharts.setCurrentRenderer(render);
		if(!FusionCharts("chartId3")){
			deptChart3 = new FusionCharts("$!{chart_path}/MSColumn3D.swf", "chartId3", "1135", "200", "0", "1" );
		}
		deptChart3.configure("ChartNoDataText", " ");
		deptChart3.setXMLUrl("$!{context_path}/man/mon/contribute/csfChart_xml.vw?year=$!{searchMap.txt2html("findYear")}&manId=" + manId + "&stratId=" + stratId);
		deptChart3.render("barCsf");
	}
//]]>
</script>

