        });
        
        /******************************************
         * 신규
         ******************************************/
        J("#btnInsert").click(function(e) {
            if(!isMngEvalCloseYn){
                goInsert();
            } else {
                alertClosing();
            }
        });
        
        /******************************************
         * 삭제
         ******************************************/
        J("#btnDelete").click(function(e) {
            if(!isMngEvalCloseYn){
                goDelete();
            } else {
                alertClosing();
            }
