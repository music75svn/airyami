								J("#scDeptNmText").append('<nobr>'+J("#findSelScDeptNm").val()+'</nobr>');
								
								var ids = J("#list").jqGrid('getDataIDs');
								var rowdata = J("#list").getRowData(ids[0]);
								
								J("#metricNmText").append("<span style='cursor:hand;overflow:hidden;text-overflow:ellipsis;width:140;'><nobr><b>"+J(rowdata.METRIC_NM).text()+"</b></nobr></span>");
								J("#findSelMetricId").val(rowdata.METRIC_ID);
								J("#findSelMetricNm").val(J(rowdata.METRIC_NM).text());
##								J("#findSelMetricId").val(J("#findSelMetricId").val());
##								J("#findSelMetricNm").val(J("#findSelMetricNm").val());
								
								/******************************************
                            	 * 지표 탭 초기화
                            	 ******************************************/
                        		tabEvent('chart');
                        		callView('chart');
								
						   },
			caption:"조직성과도"
		}).jqGrid('hideCol',"DIRECTION_NM").jqGrid('hideCol',"UP_METRIC_ID").jqGrid('hideCol',"PERSPECTIVE_NM").jqGrid('hideCol',"STRATEGY_NM").jqGrid('hideCol',"STRATEGY_ID").jqGrid('hideCol',"METRIC_ID").setGridWidth($!{jqGrid_r4_width});


		jQuery("#list2").jqGrid({
			url          :"$!{context_path}/bsc/mon/scDeptDiagram/orgRankList_xml.vw" + "?" + str,
			mtype        :"POST",
			datatype     :"json",
			jsonReader   : {
    						page   : "page",
    						total  : "total",
    						root   : "rows",
    						records: function(obj){return obj.length;},
    						repeatitems: false,
    						id     : "id"
    				       },
			height       : 460,
			width        : 227,
			colNames     :['순위', '$!{SC_DEPT_NM}', '총점', '상태' ],
			colModel     :[
							{name:'RANK'     		    ,index:'RANK'      			  ,width:20    ,align:'center' ,sorttype : 'int' },
							{name:'SC_DEPT_NM'          ,index:'SC_DEPT_NM'           ,width:120   ,align:'left'   },
                            {name:'SCORE'               ,index:'SCORE'                ,width:60   ,align:'right'  ,sorttype : 'int' },
                            {name:'STATUS'              ,index:'STATUS'               ,width:20   ,align:'center' ,formatter:linkUpdate }
							],
			rowNum       : $!{jqGrid_rownum},
			autowidth    : false,
			viewrecords  : true,
			loadonce     : false,
			multiselect  : false,
			cellEdit     : true,
			cmTemplate	 : {sortable:false},
			gridview     : true,
			caption:"조직순위"
		});
        
		/******************************************
		 * 조회
		 ******************************************/
		J("#formFind").submit(function(event) {
			//submit 하기전 작업추가
		});
        
		/******************************************
		 * 신규 등록
		 ******************************************/
		J("#btnInsert").click(function() {
			J("#mode").val("ADD");
			J("#formList").attr("action", "$!{context_path}/bsc/mon/scDeptDiagram/scDeptDiagramModify.vw?type=template").submit();
		});
        
		/******************************************
		 * 삭제 버튼 클릭 이벤트
		 ******************************************/
		J("#btnDelete").click(function() {
			goDelete();
			return false;
		});
		
		/******************************************
		 * 조직성과상세버튼 클릭 이벤트
		 ******************************************/
		J("#btnOrgDetail").click(function() {
			J("#formList").attr("action", "$!{context_path}/bsc/mon/orgDetail/orgDetailList.vw?type=template").submit();
		});
		
		/******************************************
		 * 요약보고서버튼 클릭 이벤트
		 ******************************************/
