<chart caption=''
	yAxisMaxValue='$!{searchMap.txt2html("signalTotalCnt")}'
	divLineAlpha='0'
	showValues='1'
	showLabels='0'
	showBorder='0'
	borderAlpha='0'
	canvasBorderThickness='0'
	canvasBorderAlpha='0'
	showYAxisValues='0'
	adjustDiv='0'
	canvasBgRatio='0'
	canvasBgAlpha='0'
	bgRatio='0'
	bgAlpha='0'
	numDivLines='1'
	divLineThickness='0'
	showAlternateVGridColor='1'
	showLegend='0'
	baseFontSize='20'
	baseFontColor='#FF3300'
	plotSpacePercent='0'
	chartLeftMargin='0'
	chartRightMargin='0'
	chartBottomMargin='10'
	chartTopMargin='0'
	>


   <categories>
      <category label='' />
   </categories>

   	#foreach($list in $signalCntList)
	<dataset seriesName='$!{CS_HTML.txt2html($list.CODE_NM)}'>
		<set value='$!{CS_HTML.txt2html($list.CNT)}' color='$!{CS_HTML.txt2html($list.SIG_COLOR)}'/>
    </dataset>
	#end


   <styles>
      <definition>
         <style name='CanvasAnim' type='animation' param='_xScale' start='0' duration='0' />
      </definition>
      <application>
         <apply toObject='Canvas' styles='CanvasAnim' />
      </application>
   </styles>

</chart>