		J("#userFormList input[name=findYear]").val(J("#findYear").val());
		J("#userFormList input[name=findEmpNo]").val(ret.EMP_NO_TMP);
		J("#userFormList input[name=viewEmpNm]").val(ret.EMP_NM_TMP);
		J("#userFormList input[name=findEvalGrpId]").val(ret.EVAL_GRP_ID_TMP);
		
		J("#userFormList input[name=findEmpNm]").val(J("#formFind input[name=findEmpNm]").val());
		J("#userFormList input[name=findDeptCd]").val(J("#formFind input[name=findDeptCd]").val());
