	/******************************************
	 * 일괄저장
	 ******************************************/
	function goAllSave() {
		var ids = J("#list").jqGrid('getDataIDs');
		var rowdata = "";

		if(ids.length == 0){
			alert("저장할 항목이 없습니다.");
			return false;
		}

		var evalTgtYn;
		var evalExpRsId;
		for (var i = 0; i < ids.length; i++) {
			evalTgtYn = J("#formList select[name=gEvalTgtYns]").eq(i).val();
			evalExpRsId = J("#formList select[name=gEvalExpRsIds]").eq(i).val();
			
			if(evalTgtYn == 'Y' && evalExpRsId != '99'){
				alert("평가대상여부가 대상일 경우 평가제외사유는 해당없음으로 설정해 주세요.");
				return;
			}
			if(evalTgtYn == 'N' && evalExpRsId == '99'){
				alert("평가대상여부가 미대상일 경우 평가제외사유는 해당없음이 될 수 없습니다.");
				return;
			}
		}


		J.showConfirmBox("일괄저장하시겠습니까?", "allSaveDo"); //allSaveDo() 콜백함수 호출
	}

	/******************************************
