						<tr>
							<th scope="row" style="text-align:center;"><label for="score">평가점수</label></th>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.FRS_DEPT_SCORE)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.SEC_DEPT_SCORE)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.TEAM_SCORE)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.PRS_GRADE_ITEM_NM)}</td>
							<td></td>
						</tr>
						<tr>
							<th scope="row" style="text-align:center;"><label for="convertScore">환산점수</label></th>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.FRS_DEPT_CONVERT_SCORE)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.SEC_DEPT_CONVERT_SCORE)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.TEAM_CONVERT_SCORE)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.PRS_CONVERT_SCORE)}</td>
							<td></td>
						</tr>
						<tr>
							<th scope="row" style="text-align:center;"><label for="weight">가중치</label></th>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.FRS_DEPT_WEIGHT)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.SEC_DEPT_WEIGHT)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.TEAM_WEIGHT)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.PRS_WEIGHT)}</td>
							<td></td>
						</tr>
						<tr>
							<th scope="row" style="text-align:center;"><label for="finalScore">득점</label></th>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.FRS_DEPT_WT_REFL_CONVERT_SCORE)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.SEC_DEPT_WT_REFL_CONVERT_SCORE)}</td>
							<td style="text-align:center;">$!{CS_HTML.txt2html($detailResult.TEAM_WT_REFL_CONVERT_SCORE)}</td>
