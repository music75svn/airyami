	/******************************************
     * 하단 TAB 마우스오버 이벤트
     ******************************************/	
	function tabEvent(item){
	
		hiddenAllItem();
		
		if(item=="chart"){
			J("#chart").attr("src", "$!{img_path}/common/tab2_06_on.gif");
			J("#metricChartContainer").show();
		}else{
			J("#cause").attr("src", "$!{img_path}/common/tab2_07_on.gif");
    		J("#metricCauseContainer").show();
		}
		
	}

	/******************************************
     * 하단 TAB 마우스오버 이벤트
     ******************************************/	
	function hiddenAllItem(){
	
		J("#chart").attr("src", "$!{img_path}/common/tab2_06.gif");
		J("#cause").attr("src", "$!{img_path}/common/tab2_07.gif");
		
		J("#metricChartContainer").empty();
		J("#metricCauseContainer").empty();
		
		J("#metricChartContainer").hide();
		J("#metricCauseContainer").hide();
		
	}
	
	function callView(item){
	
		if(item == 'chart'){
			callChart();
		}else{
			callCause();
		}
	}
	
	var i=0;
	var myChart1;
	
	function callChart(){
		
		var analCycle = J(":input:radio[name=findAnalCycle]:checked").val();
	
		FusionCharts.setCurrentRenderer(render);
		if(!FusionCharts("myChartId1")){
			myChart1 = new FusionCharts( "$!{chart_path}/MSColumn3DLineDY.swf", "myChartId1", "500", "250", "0", "1" );
		}
		myChart1.setXMLUrl("$!{context_path}/bsc/mon/scDeptDiagram/scDeptDiagramMetricChart_xml.vw?year=$!{searchMap.txt2html("findYear")}&mon=$!{searchMap.txt2html("findMon")}&scDeptId=$!{searchMap.txt2html("findSelScDeptId")}&analCycle="+analCycle+"&metricId="+J("#findSelMetricId").val());
    	myChart1.render("metricChartContainer");
		
	}
	
	/******************************************
	 * 조회
	 ******************************************/
    function callCause() {
		
		var year = J("#findYear").val();
		var mon  = J("#findMon").val();
		var metricId = J("#findSelMetricId").val();
		var diagramGubun = J("#findDiagramGubun").val();
		var selScDeptId = J("#findSelScDeptId").val();
		var selMetricId = J("#findSelMetricId").val();
		
		var mode = "OPNION";
		var analCycle = J(":input:radio[name=findAnalCycle]:checked").val();

		J.ajax({
        	url      : "$!{context_path}/bsc/mon/scDeptDiagram/causeMetricView.vw",
        	cache    : false,
        	type     : "POST",
			data     : "year=" + year + "&amp;mon=" + mon + "&amp;findYear=" + year + "&amp;findMon=" + mon + "&amp;metricId=" + metricId + "&amp;mode=" + mode + "&amp;analCycle=" + analCycle + "&amp;diagramGubun=" + diagramGubun + "&amp;selMetricId=" + selMetricId + "&amp;selScDeptId=" + selScDeptId,
        	dataType : "html",
        	contentType: "application/x-www-form-urlencoded; charset=UTF-8",
        	success  : function (data) {
        		J("#metricCauseContainer").empty();  //body부분 div를 empty 시켜줌(empty를 시키지 않을 경우 메모리릭 발생)   
        		J("#metricCauseContainer").append(data);
        	}
    	});
	}
	
	/******************************************
	 * 지표정의서
	 ******************************************/
	function linkUpdateMetric(cellvalue, options, rowObject) { 
	
		var str = cellvalue;
		
		if(rowObject.UP_METRIC_ID != "") {
			str = "<img src=\'$!{img_path}/tree.gif\' align='absmiddle'>" + str
		}
		
		return '<a href=javascript:goPopMetricDetail(' + options.rowId + ')>' + str + '<//a>';
	
	}
	
	/******************************************
	 * 실적상세
	 ******************************************/
	function linkUpdateActual(cellvalue, options, rowObject) { 
		return "<a href=javascript:goPopMetricActualDetail(" + options.rowId + ")>" + cellvalue+ "</a>";
	}
	
	/******************************************
	 * 실적상세
	 ******************************************/
	function linkUpdateSignal(cellvalue, options, rowObject) { 
		return "<a href=javascript:goPopMetricActualDetail(" + options.rowId + ")>" + "<img name=\'sig\'  src=\'$!{img_path}/common/ball/ball_" + cellvalue + ".gif\' >" + "</a>";
	}

	/******************************************
	 * 지표정의서버튼 클릭 이벤트
	 ******************************************/
	function goPopMetricDetail(rowId) {
		
		var ret = J("#list").jqGrid('getRowData',rowId);
		
		J("#findSelMetricId").val(ret.METRIC_ID);
		J("#findSelMetricNm").val(J(ret.METRIC_NM).text());
		
		J("#metricNmText").empty();
		J("#metricNmText").append("<span style='cursor:hand;overflow:hidden;text-overflow:ellipsis;width:80px;'><nobr><b>"+J(ret.METRIC_NM).text()+"</b></nobr></span>");
		
		tabEvent('chart');
		callView('chart');
		
	}

	/*****************************************************
	 * 실적상세 팝업
	 *****************************************************/ 
    function goPopMetricActualDetail(rowId){
		
		var ret = J("#list").jqGrid('getRowData',rowId);
		
		var scDeptId = J("#findScDeptId").val();
		var strategyId = ret.STRATEGY_ID;
		var metricId = ret.METRIC_ID;
		var year = J("#findYear :selected").val();
		var mon = J("#findMon :selected").val();
		var analCycle = J(":input:radio[name=findAnalCycle]:checked").val();
		
		var param = "&amp;year=" + year + "&amp;findMon=" + mon + "&amp;metricId=" + metricId + "&amp;analCycle="+analCycle;
		
		var url = "$!{context_path}/bsc/mon/metricActualDetail/popMetricActualDetailList.vw?type=popup" + param;
		openWinByName(url,'popMetricDetail','1250','650','yes');
		
    }
	
	
	/******************************************
	 * 전략과제 연계도 링크
	 ******************************************/
	function goStrategyMatrix(rowId) {
		var ret = J("#list").jqGrid('getRowData',rowId);
		
		J("#findStrategyId").val(ret.STRATEGY_ID);				// List에서 선택한 전략과제코드를 설정.
		J("#findStrategyNm").val(J(ret.STRATEGY_NM).text());	// List에서 선택한 전략과제명을 설정.
		J("#formList").attr("action", "$!{context_path}/bsc/mon/strategyMatrix/strategyMatrixList.vw?type=template").submit();
	}
	
	/******************************************
	 * KPI 연계도 링크
	 ******************************************/
	function goMetricMatrix(rowId) {
		var ret = J("#list").jqGrid('getRowData',rowId);
		
		J("#findMetricId").val(ret.METRIC_ID);				// List에서 선택한 KPI코드를 설정.
		J("#findMetricNm").val(J(ret.METRIC_NM).text());	// List에서 선택한 KPI명을 설정.
		J("#formList").attr("action", "$!{context_path}/bsc/mon/metricMatrix/metricMatrixList.vw?type=template").submit();
	}
	
	/******************************************
	 * KPI 성과 상세화면 링크
	 ******************************************/
	function goMetricDetail(rowId) {
		var ret = J("#list").jqGrid('getRowData',rowId);
		
		J("#year").val(J("#findYear").val());			// List에서 선택한 연도를 설정.
		J("#metricId").val(ret.METRIC_ID);				// List에서 선택한 KPI코드를 설정.
		J("#formList").attr("action", "$!{context_path}/bsc/mon/metricDetail/metricDetailList.vw?type=template").submit();
	}
	
	/******************************************
	 * jqGrid 신호등
	 ******************************************/
	function linkUpdate(cellvalue, options, rowObject) {
		return "<img name=\'sig\'  src=\'$!{img_path}/common/ball/ball_" + cellvalue + ".gif\' >";
	}
	
	/******************************************
	 * 엑셀다운로드
	 ******************************************/
	function excelDownload(){
		J("#formFind").attr("action", "$!{context_path}/bsc/mon/scDeptDiagram/scDeptDiagramListExcel.vw?type=excel").submit();
		return false;
	}
	
	/*****************************************************
	 * 조직선택 설정
	 *****************************************************/ 
	function setDeptInfoPop(scDeptId, scDeptNm) {
		scDeptNm = J(scDeptNm).text();
		J("#findScDeptId").val(scDeptId);
		J("#findScDeptNm").val(scDeptNm);
		J.fancybox.close();
	}
	
	/******************************************
	 * 그리드 ajax 조회
	 ******************************************/
	function goGridSearch(scDeptId, scDeptNm) {
		J("#findSelScDeptId").val(scDeptId);
		J("#findSelScDeptNm").val(scDeptNm);
		
		J("#scDeptNmText").empty();
		J("#metricNmText").empty();
		
		J("#list2").setGridParam({url:"$!{context_path}/bsc/mon/scDeptDiagram/orgRankList_xml.vw?"+J("#formFind").serialize()});
		J("#list2").trigger("reloadGrid");
		
		J("#list").setGridParam({url:"$!{context_path}/bsc/mon/scDeptDiagram/scDeptDiagramList_xml.vw?"+J("#formFind").serialize()}); 
		J("#list").trigger("reloadGrid");
	}
	
	/******************************************
	 * 그리드 ajax 확장, 축소
	 ******************************************/
	function hideScDetpDiagram() {
		if(J("#scDeptDiagramBox").css("display") == "block") {
			J("#scDeptDiagramBox").hide();
			J("#list").setGridHeight(350);
		} else {
			J("#scDeptDiagramBox").show();
			J("#list").setGridHeight(100);
		}
	}

//]]>
</script>

