        	success  : function (data) {
				var xmlDoc = J.parseXML(data);
        		J("#findWeightGrpId").empty();
				
				J('<option/>').attr('value', '' ).text('전체').appendTo('#findWeightGrpId');
				J('<option/>').attr('value', 'NONE' ).text('미지정').appendTo('#findWeightGrpId');
				
				J(xmlDoc).find("data").each(function() {
					J('<option/>').attr('value', J(this).find("value").text()).text(J(this).find("text").text()).appendTo('#findWeightGrpId');
