<chart 
	caption='전략과제성과상세현황(월별)' 
	subCaption='' 
	yAxisMaxValue='100' 
	numberSuffix='점'
	bgColor='EBEFF1,FFFFFF' 
	divLineColor='91AF46' 
	divLineAlpha='30' 
	alternateHGridAlpha='5' 
	canvasBorderColor='666666' 
	baseFontColor='666666'
	baseFontSize='12' 
	lineColor='91AF46' 
	numVDivlines='7' 
	showAlternateVGridColor='1' 
	anchorSides='3' 
	rotate='90'
	anchorRadius='5' 
	showValues='0'>

	<categories>
      <category label='1월' />
      <category label='2월' />
      <category label='3월' />
      <category label='4월' />
      <category label='5월' />
      <category label='6월' />
      <category label='7월' />
      <category label='8월' />
      <category label='9월' />
      <category label='10월' />
      <category label='11월' />
      <category label='12월' />
   </categories>

   
	#foreach($list in $list)
       <dataset seriesName='$!{CS_HTML.txt2html($list.METRIC_NM)}'>
          <set value='$!{CS_HTML.txt2html($list.MONTH1)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH2)}'/>
          <set value='$!{CS_HTML.txt2html($list.MONTH3)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH4)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH5)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH6)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH7)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH8)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH9)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH10)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH11)}' />
          <set value='$!{CS_HTML.txt2html($list.MONTH12)}' />
       </dataset>
	#end
	
	<trendlines>
      <line startValue='26000' color='91C728' displayValue='Target' showOnTop='1'/>
	</trendlines>
	
	<styles>
		<definition>
			<style name='Anim1' 
				type='animation' 
				param='_alpha' 
				start='0' duration='1' />
		</definition>
		<application>
			<apply toObject='TRENDLINES' 
				styles='Anim1' />
		</application>
	</styles>
</chart>