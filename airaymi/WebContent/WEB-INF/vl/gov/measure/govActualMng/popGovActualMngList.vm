    function goPopMetricActualDetail01(rowId){
		var ret = J("#list").jqGrid('getRowData',rowId);
		var url = ret.URL;
		var restricted = ret.RESTRICTED;
		if (url == "" ) {
			alert("데이터 제공지표가 아닙니다");
			return;
		}
		if (url == "X") {
			alert('선택하신 지표의 상세데이터는 가스피아의 \n[재무관리/예산관리/일반회계예산/예산통제관리/가용예산조회] 화면에서 확인해 주십시요.');
			return;
		}

		if (restricted == "Y") {
			if (!$!{CS_LOGIN.chkAuthGrp("01")} && '$!{CS_LOGIN.getSabun()}' != J("#userId").val() && '$!{CS_LOGIN.getSabun()}' != J("#approveUserId").val()) {
				alert('해당지표의 실적입력자 또는 실적승인자만 조회할 수 있습니다.');
				return;
			}
		}

		var mon = '01';
		var year = ret.YEAR;
		var metricId = ret.METRIC_ID;
		var scDeptId = ret.SC_DEPT_ID;
		var itemCd = ret.ITEM_CD;
		var param = "&amp;findYear=" + year + "&amp;findMon=" + mon + "&amp;findItemCd=" + itemCd;

		var url = "$!{context_path}/bsc/popup/govBridge/govBridgeList.vw?type=popup" + param;
		openWinByName(url,'popDetail','1250','650','yes');
	}
