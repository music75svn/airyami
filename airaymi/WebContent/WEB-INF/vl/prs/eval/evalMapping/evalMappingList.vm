	 ******************************************/
	function popInsertUser1(index) {
	
		if('$!{CS_HTML.txt2html($confirmYn)}' == 'Y'){
			J.showMsgBox("평가가 확정되어 평가자를 수정할수 없습니다.", null, null);
			return;
		}
		
		if('$!{CS_HTML.txt2html($closeYn)}' == 'Y'){
			J.showMsgBox("평가가 마감되어 평가자를 수정할수 없습니다.", null, null);
			return;
		}
		
		var ret = J("#list").jqGrid('getRowData',index);
		
		if(ret.EVAL_SUBMIT_CNT != '0'){
		
			J.showMsgBox("평가가 제출되어 평가자를 수정할수 없습니다.", null, null);
			return;
		
		}
	
		J("#index1").val(index);
		J("#anchor").attr("href","$!{context_path}/bsc/module/commModule/popSearchUser.vw?findYear=$!{searchMap.txt2html("year")}&amp;callBack=setInsertUserInfo1");
		J("#anchor").click();
	}
	
