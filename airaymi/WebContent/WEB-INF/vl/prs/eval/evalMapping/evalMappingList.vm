	function setInsertUserInfo1(arguments) {
		var index = J("#index1").val();
		J.fancybox.close();
		
		if(J("input[name=userTwoIds]").eq(index-1).val()==arguments[0] || J("input[name=userThrIds]").eq(index-1).val()==arguments[0]){
			J.showMsgBox("평가군에 평가자가 중복됩니다.", null, null); 
			return false;
		}else{
    		J("input[name=userOneIds]").eq(index-1).val(arguments[0]);
    		J("input[name=userOneNms]").eq(index-1).val(arguments[1]);
    	}
		
