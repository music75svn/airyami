<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="ko">

#set($main_url = $!{searchMap.txt2html("mainUrl")})
#set($curr_url = $!{context_path} + "/prs/eval/evalResult/popFeedDetail.vw")

<head>
	#parse("/WEB-INF/vl/common/template/head.vm")
<script type="text/javascript" language="javascript" >
//<![CDATA[

	var J = jQuery;

	J(document).ready(function(){
        
		/******************************************
		 * 닫기버튼 이벤트
		 ******************************************/
		J("#btnClose").click(function(e) {
			self.close();
		});
		
	});
	
	
//]]>
</script>    
</head>
<body>
<form id="form1" name="form1" method="post">
    <input type="hidden" 	id="mode"		    name="mode"		      value="$!{searchMap.txt2html("mode")}" />
	<input type="hidden"	id="year"           name="year"           value="$!{searchMap.txt2html("year")}" />
	$!{searchMap.makeHiddenFind()}
	
    <div>
    	<div id="Table_blockContainer_w800">
			<div class="clear h10"></div>
			
			<div>
			<table width="100%" border="0" cellspacing="0" cellpadding="0">
    		<tr>
    			<td>
    				<table width="100%" border="0" cellpadding="0" cellspacing="0" class="popup_tit_c">
    				<tr>
    					<td class="popup_tit_l"></td>
    					<td class="popup_tit_r">피드백 확인</td>
    				</tr>
    				</table>
    			</td>
    		</tr>
    		</table>	
			</div>
			
			<div class="clear h10"></div>
			
			<div class="tableTop">
                <!-- 검색 테이블 Start -->
                <div class="searchBox">
                <div><div><div>   
                    <table summary="검색조건">        
                        <caption class="none">검색조건</caption>
                        <thead><tr><th></th><th></th></tr></thead>
                        <tbody>
                            <tr>
								<td class="searchBox_tit w200">$!{YEAR} <span class="searchBar">$!{year}년</span></td>
								<td class="searchBox_tit w200">평가구분 <span class="searchBar">$!{evalDegreeNm}</span></td>
								<td class="searchBox_tit w200">평가군 <span class="searchBar">$!{evalGrpNm}</span></td>
								<td class="searchBox_tit w200">평가대상자 <span class="searchBar">$!{userNm}</span></td>
								<td class="searchBox_tit w200">평가등급 <span class="searchBar">$!{gradeNm}</span></td>
							</tr>
                        </tbody>
                    </table>
                </div></div></div>
                </div>
                <!--// 검색 테이블 End -->
                <div class="clear"></div>
            </div>
			
    		<div class="blockDetail">
    			<table summary="피드백 ">
					<caption class="none">피드백 </caption>
    				<colgroup>
    					<col width="15%" height="50"/>
    					<col width="85%" />
    				</colgroup>
    				<tbody>
						#foreach($list in $evalFeedList)
						<tr>
							<th scope="row">$!{CS_HTML.txt2html($list.EVAL_SEQ)}차 평가자<br>피드백</th>
							<td>$!{CS_HTML.txt2html($list.FEED)}</td>
						</tr>
						#end
					</tbody>
    			</table>
    		</div>
			
			<div class="blockButton"> 
    			<ul>
    				<li><input type="button" id="btnClose"   value="닫기" /></li>
    			</ul>
    		</div>	
    	</div>
		<a id="anchor"></a>
    	<div class="clear"></div>
    </div>
</form>

</body>
</html>
