<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
        "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
   
    <package name="lexken-default" extends="struts-default">
    	<interceptors>
			<interceptor-stack name="lexkenStack">
                <interceptor-ref name="exception"/>
                <interceptor-ref name="servletConfig"/>
                <interceptor-ref name="prepare"/>
                <interceptor-ref name="checkbox"/>
                <interceptor-ref name="conversionError"/>

                <interceptor-ref name="debugging"/>
			</interceptor-stack>
		</interceptors>

    	<default-interceptor-ref name="lexkenStack" />
    	
		<global-results>
			<result name="REQUEST_LOGIN">/WEB-INF/vl/secure/secure/secure/login.vm</result>
			<result name="REQUEST_MAIN">/WEB-INF/vl/bsc/mon/main/mainList.vm</result>
			<result name="ERROR_PAGE">/WEB-INF/vl/bsc/module/commModule/errorPage.vm</result>
		</global-results>
    
    </package>
    
</struts>


