                            {name:'EVAL_MEMB_EMPN_SEQ',index:'EVAL_MEMB_EMPN_SEQ',width:70       ,align:'center', sortable:true },
							{name:'KOR_NM'   		,index:'KOR_NM'				,width:40   	,align:'center', sortable:true ,formatter:linkUpdate},
							{name:'DEPT_CD'			,index:'DEPT_CD'			,width:150		,align:'center', sortable:true },
