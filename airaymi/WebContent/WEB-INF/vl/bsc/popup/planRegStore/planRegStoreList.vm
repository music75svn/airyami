<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" lang="ko">
	
#set($curr_url = $!{context_path} + "/bsc/popup/planRegStore/planRegStoreList.vw")
<head>
	#parse("/WEB-INF/vl/common/template/head.vm")
<script type="text/javascript" language="javascript" >
//<![CDATA[

	var J = jQuery;
	
	J(document).ready(function(){
    
		/******************************************
		 * 데이터 그리드 출력
		 ******************************************/
		var str = J('#formFind').serialize();
		
		jQuery("#list").jqGrid({
			url          :"$!{context_path}/bsc/popup/planRegStore/planRegStoreList_xml.vw" + "?" + str,
			mtype        :"POST",
			datatype     :"json",
			jsonReader   : {   
    						page   : "page",
    						total  : "total",
    						root   : "rows", 
    						records: function(obj){return obj.length;},
    						repeatitems: false, 
    						id     : "id"
    				       },
			height       : $!{jqGrid_height},
			width        : $!{jqGrid_width},
			colNames     :['연도', '월', '부서번호', '부서', '사원번호', '이름', '허가구분', '계획등록업소수', '정기검사(정기점검)업소수'
						 , '정기검사검사건수', '정기검사계획건수', '정기검사계획등록률', '정기점검검사건수', '정기점검계획건수' ,'정기점검계획등록률'],
			colModel     :[
							{name:'YEAR'           		,index:'YEAR'      	 		,width:50   	,align:'center' }, 
                            {name:'MON'       			,index:'MON'         		,width:50   	,align:'center' }, 
                            {name:'BR_CD'       		,index:'BR_CD'         		,width:60   	,align:'center' },
							{name:'BR_NM'       		,index:'BR_NM'        		,width:50    	,align:'center' },
                            {name:'EMPN'       			,index:'EMPN'       		,width:50    	,align:'center' }, 
                            {name:'EMPN_NM'       		,index:'EMPN_NM'       		,width:50    	,align:'center' }, 
                            {name:'PERM_NM'       		,index:'PERM_NM'         	,width:50   	,align:'center' }, 
                            {name:'PLAN_QTY'      		,index:'PLAN_QTY'     		,width:50   	,align:'center' },
                            {name:'INSP_QTY'      		,index:'INSP_QTY'     		,width:70   	,align:'center' },
                            {name:'C1_INSP_QTY'      	,index:'C1_INSP_QTY'     	,width:50   	,align:'center' },
                            {name:'C1_PLAN_QTY'     	,index:'C1_PLAN_QTY'    	,width:50   	,align:'center' },
                            {name:'C1_P'      			,index:'C1_P'     			,width:60  		,align:'center' },
                            {name:'C2_INSP_QTY'      	,index:'C2_INSP_QTY'     	,width:60  		,align:'center' },
                            {name:'C2_PLAN_QTY'      	,index:'C2_PLAN_QTY'     	,width:60  		,align:'center' },
                            {name:'C2_P'      			,index:'C2_P'     			,width:60   	,align:'center' }
							],
			rowNum       : $!{jqGrid_rownum},
			autowidth    : false, 
			viewrecords  : true,
			loadonce     : true,
			multiselect  : false,
			cmTemplate	 : {sortable:false},
			cellEdit     : true,
			caption:"시스템 연계데이터 조회"
		}).jqGrid('hideCol',"BR_CD").jqGrid('hideCol',"EMPN").jqGrid('hideCol',"C1_INSP_QTY")
		  .jqGrid('hideCol',"C1_PLAN_QTY").jqGrid('hideCol',"C1_P").jqGrid('hideCol',"C2_INSP_QTY").jqGrid('hideCol',"C2_PLAN_QTY").jqGrid('hideCol',"C2_P").setGridWidth($!{jqGrid_width});
        
		/******************************************
		 * fancybox 설정
		 ******************************************/
		J("#anchor").fancybox();    //조직선택 팝업
		
		/******************************************
		 * 엑셀다운로드 버튼 클릭 이벤트
		 ******************************************/
		J("#btnExcel").click(function() {
			J("#yearNm").val(J("#findYear > option:selected").text());
			J("#monNm").val(J("#findMon > option:selected").text());
			J("#deptNm").val(J("#findDeptId > option:selected").text());
			J("#itemNm").val(J("#findItemCd > option:selected").text());
			J("#formFind").attr("action", "$!{context_path}/bsc/popup/planRegStore/planRegStore/excel.vw?type=excel").submit();
			J("#formFind").attr("action", "$!{context_path}/bsc/popup/planRegStore/planRegStoreList.vw?type=popup");
			
			return false;
		});
		
	});
	

//]]>
</script>
</head>
<body>
<div id="Table">
	<div id="Table_blockContainer">
		<h1 class="none">시스템 연계데이터 조회</h1>
		<div class="tableTop">
			#parse("/WEB-INF/vl/common/template/tabs.vm")
			
    		<!-- search S -->
			<form id="formFind" name="formFind" method="post" action="$!{context_path}/bsc/popup/planRegStore/planRegStoreList.vw?type=template">
				<input type="hidden"	id="findHighPgmId"	name="findHighPgmId"	value="$!{searchMap.txt2html("findHighPgmId")}" />
				<input type="hidden" 	id="findSubPgmId"	name="findSubPgmId"		value="$!{searchMap.txt2html("findSubPgmId")}" />
				<input type="hidden"    id="findPgmId"		name="findPgmId"		value="$!{searchMap.txt2html("findPgmId")}" />
				<input type="hidden" 	id="yearNm"			name="yearNm" />
				<input type="hidden" 	id="monNm"			name="monNm" />
				<input type="hidden" 	id="deptNm"			name="deptNm" />
				<input type="hidden" 	id="itemNm"			name="itemNm" />
				<input type="hidden" 	id="mode"			name="mode" />
				
				<div class="searchBox">
    				<div><div><div>
    				<table summary="시스템 연계데이터 조회검색 조건">
						<caption class="none">시스템 연계데이터 조회 검색 조건</caption> 
						<thead><tr><th></th></tr></thead>
        				<tbody>
            				<tr>
								<td class="searchBox_tit w280">
            						<span><label for="findYear">평가년도</label></span>
            						<span class="searchBar">
            							<select id="findYear" name="findYear" class="select w80">
            								$!CS_CODE.getCodeOption("017", $!{searchMap.txt2html("findYear")})
            							</select>
            							<select id="findMon" name="findMon" class="select w70">
            								<option value="">전체</option>
            								$!CS_CODE.getCodeOption("024", $!{searchMap.txt2html("findMon")})
            							</select>
            					
            						</span>
            					</td>
            					<td class="searchBox_tit w300">
                					<span><label for="findDeptId">부서</label></span>
            						<span class="searchBar">
										<select id="findDeptId" name="findDeptId" class="select w250">
											#foreach($list in $deptList)
                                            	<option value="$!{CS_HTML.txt2html($list.DEPT_ID)}" $searchMap.getSelected("findDeptId", "$!{CS_HTML.txt2html($list.DEPT_ID)}")>$!{CS_HTML.txt2html($list.DEPT_NM)}</option>
                                            #end
										</select>
            						</span>
                                </td>
                                <td class="searchBox_tit w270">
                					<span><label for="findItemCd">항목</label></span>
									<span class="searchBar">
										<select id="findItemCd" name="findItemCd" class="select w200">
											#foreach($item in $itemInfo)
												<option value="$!{CS_HTML.txt2html($item.ITEM_CD)}" $searchMap.getSelected("findItemCd", "$!{CS_HTML.txt2html($item.ITEM_CD)}")>$!{CS_HTML.txt2html($item.CAL_TYPE_COL_NM)}</option>
											#end
            							</select>
									</span>
                                </td>
            				</tr>
        				</tbody>
        				<div class="blockButton " style="position:absolute;right:10px; width:150px;float:right; align:middle;padding: 0 10px 0 0;" >
                    		<input id="btnExcel" type="button" align="middle" value="엑셀다운로드", alt="엑셀다운로드" />
                    	</div>
    				</table>
                    <input type="image" class="btnSearch" src="$!{img_path}/common/btn_search.gif" alt="검색" style="position:absolute;right:120px;"/> 
                    </div></div></div>
    	        </div>
    		</form>
    		<!-- search E -->
			<a id="anchor" ></a>
			<div class="clear"></div>
		</div>
		
		<form id="formList" name="formList" method="post" action="$!{context_path}/bsc/popup/planRegStore/planRegStoreProcess.vw?type=template">
			<input type="hidden" id="mode"       name="mode" />
			$!{searchMap.makeHiddenFind()}
			<div id="splitterBox">
				<div>
					<!-- 데이터그리드 Start -->
					<div class="blockGridListTable">
						<table id="list" summary="시스템 연계데이터 조회 조회결과">
							<caption class="none">시스템 연계데이터 조회 조회</caption>
							<thead><tr><th></th></tr></thead>
							<tbody><tr><td></td></tr></tbody>
						</table>
						<div id="pager"></div>
					</div>
					<!--// 데이터그리드 End -->
				</div>
			</div>
		</form>
	</div>
	<div class="clear"></div>
</div>
</body>
</html>
